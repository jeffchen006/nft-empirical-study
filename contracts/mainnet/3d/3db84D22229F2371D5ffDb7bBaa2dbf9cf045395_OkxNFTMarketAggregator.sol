                    or(
                        iszero(returndatasize()),                  // no return data, or
                        and(
                            iszero(lt(returndatasize(), 32)),      // at least 32 bytes
                            eq(mload(0), 1)                       // starts with uint256(1)
                        )
                    )
                )
                // Restore the original free memory pointer.
                mstore(FreeMemoryPointerSlot, memPointer)
