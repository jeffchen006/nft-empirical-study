// File contractsNonUpgreadable/ElevateMarket.sol

pragma solidity ^0.8.4;

contract ElevateMarket is Pausable, AccessControl {
    using SafeMath for uint256;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    address public commisionReceiver;
    uint256 public commisionPercentage;

    bool public enabledRoyalty = false;

    struct Sale {
        address owner;
        IERC721 contractAddress;
        uint256 tokenId;
        uint256 price;
    }

    mapping(IERC721 => mapping(uint256 => Sale)) saleTokens;

    event ListTokenForSale(
        address owner,
        IERC721 contractAddress,
        uint256 tokenId,
        uint256 price
    );
    event BuyToken(
        address receiver,
        IERC721 contractAddress,
        uint256 tokenId,
        uint256 price
    );
    event SetCommisionReceiver(address account);
    event SetCommissionPercentage(uint256 percentage);
    event CancelListing(IERC721 contractAddress, uint256 tokenId);
    event RoyaltyEnabled(bool value);

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
    }

    function setEnabledRoyalty(bool value) public onlyRole(DEFAULT_ADMIN_ROLE) {
        enabledRoyalty = value;
        emit RoyaltyEnabled(value);
    }

    function listNFTForSale(
        IERC721 contractAddress,
        uint256 tokenId,
        uint256 price
    ) public {
        require(
            contractAddress.ownerOf(tokenId) == msg.sender,
            "Elevate Market: Invalid Token Owner"
        );
        require(
            contractAddress.getApproved(tokenId) == address(this),
            "Elevate Market: Token not approved"
        );
        //contractAddress.transferFrom(msg.sender, address(this), tokenId);

        Sale storage sale = saleTokens[contractAddress][tokenId];
        sale.owner = msg.sender;
        sale.contractAddress = contractAddress;
        sale.tokenId = tokenId;
        sale.price = price;

        emit ListTokenForSale(msg.sender, contractAddress, tokenId, price);
    }

    function buyToken(
        IERC721 contractAddress,
        uint256 tokenId,
        address receiver
    ) public payable {
        Sale storage sale = saleTokens[contractAddress][tokenId];
        require(
            sale.contractAddress == contractAddress,
            "Elevate Market: Invalid Token"
        );
        require(sale.tokenId == tokenId, "Elevate Market: Invalid token id");
        require(
            msg.value == sale.price,
            "Elevate Market: Price value mismatched"
        );
        require(
            sale.owner == contractAddress.ownerOf(tokenId),
            "Elevate Market: Invalid token owner"
        );

        uint256 royaltyFee;
        address royaltyReceiver;
        if(enabledRoyalty) {
            bool supportsRoyaltyInterface = contractAddress.supportsInterface(0x2a55205a);
            if(supportsRoyaltyInterface) {
                (royaltyReceiver, royaltyFee) = contractAddress.royaltyInfo(tokenId, sale.price);
            }
        }

        uint256 commission = sale.price.mul(commisionPercentage).div(10**2);
        uint256 salePrice = sale.price - commission - royaltyFee;
        
        (bool success, ) = sale.owner.call{value: salePrice}(
            ""
        );
        require(success, "Elevate Market: Fail to send sellers cut");
        (
            bool commissionSuccess,
            
        ) = commisionReceiver.call{value: commission}("");
        require(commissionSuccess, "Elevate Market: Fail to send commission");

        (bool royaltySuccess, ) = royaltyReceiver.call{value: royaltyFee}("");
        require(royaltySuccess, "Elevate Market: Fail to send royalty");

        contractAddress.transferFrom(sale.owner, receiver, sale.tokenId);
        delete saleTokens[contractAddress][tokenId];
        emit BuyToken(receiver, contractAddress, tokenId, salePrice);
    }

    function cancelListing(IERC721 contractAddress, uint256 tokenId) public {
        Sale storage tokenSale = saleTokens[contractAddress][tokenId];
        require(tokenSale.owner == msg.sender, "Elevate Market: Invalid owner");
        require(
            tokenSale.contractAddress == contractAddress,
            "Elevate Market: Invalid Token"
        );
        require(
            tokenSale.tokenId == tokenId,
            "Elevate Market: Invalid token id"
        );
        //contractAddress.transferFrom(address(this), tokenSale.owner, tokenId);
        delete saleTokens[contractAddress][tokenId];

        emit CancelListing(contractAddress, tokenId);
    }

    function withdraw() public onlyRole(DEFAULT_ADMIN_ROLE) {
        (bool sent, ) = msg.sender.call{
            value: address(this).balance
        }("");
        require(sent, "Elevate Market: Withdraw failed");
    }

    function getListedToken(IERC721 contractAddress, uint256 tokenId)
        public
        view
        returns (Sale memory sale)
    {
        return saleTokens[contractAddress][tokenId];
    }

    function setCommissionReceiver(address account)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(account != address(0), "Elevate Market: Invalid Account");
        commisionReceiver = account;

        emit SetCommisionReceiver(account);
    }

    function setCommissionPercentage(uint256 percentage)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(
            percentage > 0 && percentage <= 100,
            "Elevate Market: Invalid percentage"
        );
        commisionPercentage = percentage;

        emit SetCommissionPercentage(percentage);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}