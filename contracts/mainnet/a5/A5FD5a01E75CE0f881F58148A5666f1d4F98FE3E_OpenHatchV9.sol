    //helpers
    function sum_array(uint256[] memory array, uint256 total)
        internal
        pure
        returns (bool)
    {
        uint256 sum = 0;
        for (uint256 i = 0; i < array.length; i++) {
            sum = array[i] + sum;
        }

        if (sum > total) {
            return false;
        } else {
            return true;
        }
    }



