    }

    function owner() public view virtual returns (address) {
        return _owner;
    }

    modifier onlyOwner() {
        require(owner() == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}

pragma solidity ^0.8.0;

contract SkyDancers is ERC721, Ownable {

    using SafeMath for uint256;

    uint256 public constant price = 40000000000000000; //0.04 ETH

    uint256 public nextTokenId;

    uint public constant maxPurchase = 4;

    uint256 public MAX_SUPPLY;

    bool public isPublicSaleActive = false;

    bool public isAllowListActive = false;

    mapping(address => uint8) private _allowList;

    bytes32 immutable public merkleRoot;

    mapping(address => bool) public claimed;

    constructor(string memory name, string memory symbol, uint256 maxNftSupply, bytes32 _merkleRoot) ERC721(name, symbol) {
        MAX_SUPPLY = maxNftSupply;
        merkleRoot = _merkleRoot;
    }

    function withdraw() public onlyOwner {
        uint balance = address(this).balance;
        payable(address(msg.sender)).transfer(balance);
    }

    function setAllowList(address[] calldata addresses, uint8 numAllowedToMint) external onlyOwner {
        for (uint256 i = 0; i < addresses.length; i++) {
            _allowList[addresses[i]] = numAllowedToMint;
        }
    }

    function flipSaleState() public onlyOwner {
        isPublicSaleActive = !isPublicSaleActive;
    }

    function flipAllowListState() public onlyOwner {
        isAllowListActive = !isAllowListActive;
    }

    function toBytes32(address addr) pure internal returns (bytes32) {
    return bytes32(uint256(uint160(addr)));
  }

    function mint(uint numberOfTokens) public payable {
        require(isPublicSaleActive, "Sale must be active to mint token");
        require(numberOfTokens <= maxPurchase, "Can only mint 5 tokens at a time");
        require(price.mul(numberOfTokens) <= msg.value, "Ether value sent is not correct");
        require(nextTokenId <= MAX_SUPPLY);
        for(uint i = 0; i < numberOfTokens; i++) {
            nextTokenId++;
            _safeMint(msg.sender, nextTokenId);
        }
    }

    function mintAllowList(bytes32[] calldata merkleProof) public payable{
        require(isAllowListActive, "Allow list is not active");
        require(claimed[msg.sender] == false, "already claimed");
        require(nextTokenId <= MAX_SUPPLY);
        claimed[msg.sender] = true;
        require(MerkleProof.verify(merkleProof, merkleRoot, keccak256(abi.encodePacked(msg.sender))) == true, "invalid merkle proof");
        nextTokenId++;
        _safeMint(msg.sender, nextTokenId);
    }
}