/**
 * @title GorjsToken
 * @notice ERC20 based yielding token, rewarded to users who hold mint pass NFTs.
 * @author Alan M.
 * @dev Implementation of yielding token based on ERC20Upgradeable
 */
// AUDIT: LCL-01 | CENTRALIZED CONTROL OF CONTRACT UPGRADE Category
/// @dev we transfer ownership of proxyAmdin to multi-sig public wallet
// AUDIT: LCL-05 | MISSING INHERITANCE
contract GorjsToken is  
    Initializable, 
    IGorjsToken,
    ERC20Upgradeable, 
    ERC20PermitUpgradeable, 
    ERC20VotesUpgradeable, 
    OwnableUpgradeable, 
    AccessRoleUpgradeable  
{
    /// @notice Emitted when rewards are claimed by an account.
    event RewardClaimed(
        address indexed account, 
        address indexed source, 
        uint256 amount
    );

    /// @notice Emitted when transfer restriction is set/unset.
    event SetRestriction(
        bool isRestricted
    );
    
    /// @notice Emitted when vesting contract is set.
    event SetVestingContract(
        address vestingContract
    );

    /// @notice Emitted when voting contract is set.
    event SetVotingContract(
        address votingContract
    );

    /// @notice Emitted when transfer whitelist address is set/unset.
    event SetWhitelist(
        address indexed account,
        bool isWhitelisted
    );

    /// @notice Emitted when NFT reward whitelist address is set/unset.
    event SetRewardsWhitelist(
        address indexed account,
        bool isWhitelisted
    );

    /// @notice Emitted when user's token yielding reward amount is updated.
    event UpdateRewards(
        address indexed account,
        uint256 amount
    );

    /// @notice Emitted when user's token is locked.
    event LockToken(
        address indexed account, 
        uint256 indexed proposalId, 
        uint256 amount
    );

    struct LockInfo {
        uint256 proposalId;
        uint256 amount;
    }

    /// @dev address => address => rewards
    mapping(address => mapping(address => uint256)) private _rewardsToClaim;

    /// @dev Mapping from account to block number, which represents the block number the rewards are updated for the last time.
    mapping(address => mapping(address => uint256)) public rewardsUpdatedBlock;

    /// @notice mapping for transfer whitelisted addressses
    mapping(address => bool) public transferWhitelist;

    /// @notice mapping for rewards whitelisted addressses
    mapping(address => bool) public rewardsWhitelist;

    /// @notice mappping for token lock, user_address => array of lock info
    mapping(address => LockInfo[]) public userLocks;

    /// @dev Smart contract for mint pass.
    IGORJSAccessPass private _mintPassContract;
    
    // AUDIT: GTL-01 | INITIAL TOKEN DISTRIBUTION
    /// @dev artistReferralWallet, reserveWallet are multi-sig wallets that managed by founding team.
    /// @dev vesting contract holds the tokens(38%) for multi-sig wallets - founding team wallet, advisory team wallet, creator's token grant wallet.
    /// @dev the total supply of dao token.
    uint256 private constant TOTAL_SUPPLY = 10 ** 8 * 10 ** 18;

    /// @dev token allocation percent for each wallets.
    uint256 private constant COMMUNITY_SHARE = 40;
    uint256 private constant VESTING_CONTRACT_SHARE = 38;
    uint256 private constant ARTIST_REFERRAL_SHARE = 2;
    uint256 private constant RESERVE_SHARE = 20;

    /// @dev Base rewards for mint pass, that represents amount of rewards per token per block.
    uint256 private _baseMintPassRewards;

    /// @dev Base rewards for creator NFT, that represents amount of rewards per token per block.
    uint256 private _baseCreatorNFTRewards;

    /// @dev Amount of rewards issued when new mint pass tokens are minted.
    uint256 private _initialIssuance;

    /// @dev Reserve wallet
    address private _reserveWallet;
    /// @dev Artist token referral wallet
    address private _artistReferralWallet;
    /// @dev vesting contract;
    address private _vestingContract;
    /// @dev voting contract
    address private _votingContract;

    /// @notice transfer restriction switch
    bool public isTransferRestricted;

    // AUDIT: LCL-02 | UNPROTECTED INITIALIZER
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    /**
     * @dev Initializer for upgradeable smart contract.
     * @param mintPass_ The address of mint pass smart contract.
     * @param vestingContract_ The address of mint pass smart contract.
     * @param reserveWallet_ The address of mint pass smart contract.
     * @param artistReferralWallet_ The address of mint pass smart contract.
     * @param baseMintPassRewards_ The base rewards.
     * @param baseCreatorNFTRewards_ The base rewards.
     * @param initialIssuance_ Initial issuance for new mint pass tokens.
     * @param adminWallet The wallet address of admin
     */
    // AUDIT: LCL-07 | FUNCTION SHOULD BE DECLARED EXTERNAL
    function initialize(
        address mintPass_, 
        address vestingContract_, 
        address reserveWallet_, 
        address artistReferralWallet_, 
        uint256 baseMintPassRewards_, 
        uint256 baseCreatorNFTRewards_, 
        uint256 initialIssuance_,
        address adminWallet
    ) external initializer {
        require(mintPass_ != address(0), "invalid mintPass");
        require(vestingContract_ != address(0), "invalid vesting Contract");
        require(reserveWallet_ != address(0), "invalid reserveWallet");
        require(artistReferralWallet_ != address(0), "invalid artistReferralWallet");

        __Context_init();
        __ERC20_init("GorjsToken", "GORJS");
        __ERC20Permit_init("GorjsToken");
        __ERC20Votes_init();
         __Ownable_init();
        __AccessRole_init(adminWallet);
        // initialize state variables
        _baseMintPassRewards = baseMintPassRewards_;
        _baseCreatorNFTRewards = baseCreatorNFTRewards_;
        _initialIssuance = initialIssuance_;

        _vestingContract = vestingContract_;
        _reserveWallet = reserveWallet_;
        _artistReferralWallet = artistReferralWallet_;
        // premint tokens
        _mint(address(this), TOTAL_SUPPLY * COMMUNITY_SHARE / 100);
        _mint(_vestingContract, TOTAL_SUPPLY * VESTING_CONTRACT_SHARE / 100);
        _mint(_artistReferralWallet, TOTAL_SUPPLY * ARTIST_REFERRAL_SHARE / 100);
        _mint(_reserveWallet, TOTAL_SUPPLY * RESERVE_SHARE / 100);
        // add addresses to whitelist
        transferWhitelist[_vestingContract] = true;
        transferWhitelist[address(this)] = true;
        transferWhitelist[_artistReferralWallet] = true;
        // mint GORJS mint pass
        _mintPassContract = IGORJSAccessPass(mintPass_);
        rewardsWhitelist[mintPass_] = true;
        // set retriction on transfer
        isTransferRestricted = true;
    }

    /**
     * @dev Modifier that checks if msg sender is access pass or creator's nft address.
     */
    modifier onlyRewardsWL() {
        require(rewardsWhitelist[msg.sender], "Not in Rewards Whitelist");
        _;
    }

    /**
     * @dev Modifier that checks if msg sender is voting smart contract.
     */
    modifier onlyVotingContract() {
        require(msg.sender == _votingContract, "Not Voting Contract.");
        _;
    }

    /**
     * @dev Method that returns the claimable reward amount for user.
     * @param account The account to get claimable rewards.
     * @param source The account of rewards source.
     * @return the amount of rewards to claim.
     */
    function getClaimableRewards(address account, address source) external view returns(uint256) {
        require(rewardsWhitelist[source], "Not in Rewards Whitelist");

        return _rewardsToClaim[account][source] + getAccumulatedRewards(account, source);
    }

    /**
     * @dev Rewards can only be claimed by mint pass token.
     * @param account The account to claim rewards.
     */
    // AUDIT: LCL-03 | ENOUGH REWARD TOKENS SHOULD BE GUARANTEED IN THE CONTRACT
    /// @dev Based on tokenomic, we send 38% of supply to Vesting contract and 40% to token itself for token yielding.
    function claimRewards(address account) override external onlyRewardsWL {
        updateRewards(account);
        uint256 balance =  balanceOf(address(this));
        uint256 claimedRewards = _rewardsToClaim[account][msg.sender];

        // AUDIT: GTL-02 | LOGICAL ISSUE OF claimRewards()
        // just in case if rounding error causes not enough token for yield bearing.
        if (claimedRewards < balance) {
            _transfer(address(this), account, claimedRewards);
            _rewardsToClaim[account][msg.sender] = 0;
        } else {
            _transfer(address(this), account, balance);
            _rewardsToClaim[account][msg.sender] = claimedRewards - balance;
        }

        emit RewardClaimed(account, msg.sender, claimedRewards);
    }

    /**
     * @dev Rewards can only be updated by mint pass token.
     * @param account The account to update rewards.
     * @param amountMinted The amount of tokens minted.
     */
    function updateRewardsOnMint(address account, uint256 amountMinted) override external onlyRewardsWL {
        // if not mint pass, do not calculate_initialIssuance
        // Add rewards based on number of tokens minted.
        if(msg.sender == address(_mintPassContract)) {
            _rewardsToClaim[account][msg.sender] += amountMinted * _initialIssuance;
        }

        updateRewards(account);
    }

    /**
     * @dev Rewards can only be updated by mint pass token.
     * @param from The account that token is transferred from.
     * @param to The account that token is transferred to.
     */
    function updateRewardsOnTransfer(address from, address to) override external onlyRewardsWL {
        updateRewards(from);
        updateRewards(to);
    }

    /**
     * @dev Swtich transfer restriction toggle
     * @param _isRestricted boolean value to represent restriction
     */
    function setRestriction(bool _isRestricted) external onlyAdmin {
        isTransferRestricted = _isRestricted;
        emit SetRestriction(_isRestricted);
    }

    /**
     * @dev Set vesting contract
     * @param vestingContract_ the address of vesting contract
     */
    function setVestingContract(address vestingContract_) external onlyAdmin {
        require(vestingContract_ != address(0), "Invalid address");
        _vestingContract = vestingContract_;
        emit SetVestingContract(vestingContract_);
    }

    /**
     * @dev Set voting contract
     * @param votingContract_ the address of voting contract
     */
    function setVotingContract(address votingContract_) external onlyAdmin {
        require(votingContract_ != address(0), "Invalid address");
        _votingContract = votingContract_;
        emit SetVotingContract(votingContract_);
    }

    /**
     * @dev Set an address as whitelist
     * @param account address for whitelist in transfer
     * @param _isWhitelisted boolean value to represent restriction
     */
    function setWhitelist(address account, bool _isWhitelisted) external onlyAdmin {
        require(account != address(0), "Invalid address");
        transferWhitelist[account] = _isWhitelisted;
        emit SetWhitelist(account, _isWhitelisted);
    }

    /**
     * @dev Set an address to rewards whitelist
     * @param account NFT address
     * @param _isWhitelisted boolean value to represent restriction
     */
    function setRewardsWhitelist(address account, bool _isWhitelisted) external onlyAdmin {
        require(account != address(0), "Invalid address");
        rewardsWhitelist[account] = _isWhitelisted;
        emit SetRewardsWhitelist(account, _isWhitelisted);
    }
    
    /**
     * @dev Lock token once user vote to proposal
     * @param account the address of user
     */
    function lockToken(address account, uint256 proposalId, uint256 amount) override external onlyVotingContract {
        uint256 lockedAmount = getLockedAmount(account);
        require(balanceOf(account) - lockedAmount >= amount, "Not enough balance");
        // lock token into a proposal
        LockInfo memory lockInfo = LockInfo(proposalId, amount);
        userLocks[account].push(lockInfo);

        // AUDIT: LCL-04 | MISSING EMIT EVENTS
        emit LockToken(account, proposalId, amount);
    }

    /**
     * @dev Override ERC20 transfer function in order to set restriction
     */
    function transfer(address to, uint256 amount) public override(IERC20Upgradeable, ERC20Upgradeable) returns (bool) {
        require(!isTransferRestricted || transferWhitelist[msg.sender] || transferWhitelist[to], "From or To should be in whitelist");

        _transfer(msg.sender, to, amount);
        return true;
    }

    /**
     * @dev Override ERC20 transferFrom function in order to set restriction
     */
    function transferFrom(address from, address to, uint256 amount) public override(IERC20Upgradeable, ERC20Upgradeable) returns (bool) {
        require(!isTransferRestricted || transferWhitelist[from] || transferWhitelist[to], "From or To should be in whitelist");

        _spendAllowance(from, msg.sender, amount);
        _transfer(from, to, amount);
        return true;
    }
    
    /**
     * @dev Method to returns the total locked amount for account
     * @param account the address of user
     */
    function getLockedAmount(address account) override public view returns(uint256) {
        LockInfo[] memory userLockInfo = userLocks[account];
        uint256 lockedAmounts;
        
        for(uint256 i = 0; i < userLockInfo.length; i ++) {
            lockedAmounts += userLockInfo[i].amount;
        }
        return lockedAmounts;
    }

    /**
     * @dev Unlock tokens if proposal is declined/accepted.
     * @param account the address of user
     */
    function unlockToken(address account) override public {
        LockInfo[] storage userLockInfo = userLocks[account];
        uint256 i;
        while(true) {
            if(i >= userLockInfo.length) {
                break;
            }
            uint256 proposalId = userLockInfo[i].proposalId;

            // AUDIT: GTL-03 | UNKNOWN IMPLEMENTATION
            // dev: this functionality is used in phase2 voting system. we will audit phase 2 smart contract also.

            // if proposal is ended, unlock it
            bool isProposalEnded = IVotingProposalStatus(_votingContract).isProposalEnded(proposalId);
            if(isProposalEnded) {
                userLockInfo[i] = userLockInfo[userLockInfo.length - 1];
                userLockInfo.pop();
            } else {
                i++;
            }
        }
    }

    /// @notice internal functions
    
    /**
     * @dev Internal method to update rewards of an account.
     * @param account The account to update rewards.
     */
    function updateRewards(address account) internal {
        uint256 accumulatedRewards = getAccumulatedRewards(account, msg.sender);

        _rewardsToClaim[account][msg.sender] += accumulatedRewards;
        rewardsUpdatedBlock[account][msg.sender] = block.number;

        // AUDIT: LCL-04 | MISSING EMIT EVENTS
        emit UpdateRewards(account, _rewardsToClaim[account][msg.sender]);
    }

    /**
     * @dev Internal method to get accumulated rewards.
     * @param account The account to get accumulated rewards.
     * @param source The account of rewards source
     */
    function getAccumulatedRewards(address account, address source) internal view returns(uint256) {
        if (rewardsUpdatedBlock[account][source] == 0) {
            return 0;
        }

        // if mint pass
        if(source == address(_mintPassContract)) {
            return IERC721(source).balanceOf(account) * (block.number - rewardsUpdatedBlock[account][source]) * _baseMintPassRewards;
        } else {
            // if creator NFT is ERC721
            if(IERC165(source).supportsInterface(0x80ac58cd)) {
                return IERC721(source).balanceOf(account) * (block.number - rewardsUpdatedBlock[account][source]) * _baseCreatorNFTRewards;
            } else {
                return IERC1155EnumerableUpgradeable(source).totalBalanceOf(account) * (block.number - rewardsUpdatedBlock[account][source]) * _baseCreatorNFTRewards;
            }
        }
    }

    function _afterTokenTransfer(address from, address to, uint256 amount)
        internal
        override(ERC20Upgradeable, ERC20VotesUpgradeable)
    {
        super._afterTokenTransfer(from, to, amount);
    }

    function _beforeTokenTransfer(address from, address to, uint256 amount)
        internal
        override(ERC20Upgradeable)
    {
        unlockToken(from);
        uint256 lockedAmount = getLockedAmount(from);
        // check if its mint or has sufficient unlocked balance
        require(from == address(0) || (balanceOf(from) - lockedAmount) >= amount, "Insufficient unlocked amount");
        super._beforeTokenTransfer(from, to, amount);
    }

    function _mint(address to, uint256 amount)
        internal
        override(ERC20Upgradeable, ERC20VotesUpgradeable)
    {
        super._mint(to, amount);
    }

    function _burn(address account, uint256 amount)
        internal
        override(ERC20Upgradeable, ERC20VotesUpgradeable)
    {
        super._burn(account, amount);
    }
}

