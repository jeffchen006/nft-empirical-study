// File: contracts/TimelockStorage.sol

pragma solidity ^0.5.16;

contract TimelockStorage {
    uint256 public constant GRACE_PERIOD = 14 days;
    uint256 public constant MINIMUM_DELAY = 2 days;
    uint256 public constant MAXIMUM_DELAY = 30 days;

    address public admin;
    address public pendingAdmin;
    uint256 public delay;

    mapping(bytes32 => bool) public queuedTransactions;

    address pendingTimelockImplementation;
    address timelockImplementation;

    //makes sense to include events here, not in interface, because proxy doesn't need interface, while a concrete implementation and proxy need storage
    event NewAdmin(address indexed newAdmin);
    event NewPendingAdmin(address indexed newPendingAdmin);
    event NewDelay(uint256 indexed newDelay);
    event CancelTransaction(
        bytes32 indexed txHash,
        address indexed target,
        uint256 value,
        string signature,
        bytes data,
        uint256 eta
    );
    event ExecuteTransaction(
        bytes32 indexed txHash,
        address indexed target,
        uint256 value,
        string signature,
        bytes data,
        uint256 eta
    );
    event QueueTransaction(
        bytes32 indexed txHash,
        address indexed target,
        uint256 value,
        string signature,
        bytes data,
        uint256 eta
    );

    event NewPendingImplementation(
        address oldPendingImplementation,
        address newPendingImplementation
    );
    event NewImplementation(
        address oldImplementation,
        address newImplementation
    );
}

