pragma solidity ^0.5.16;



/**
 * @title TimelockDelegator
 * @dev Storage for the Timelock is at this address, while execution is delegated to the `timelockImplementation`.
 */
contract TimelockDelegator is TimelockStorage,ProxyErrorReporter{

    constructor(address initialTimelockImplementation,uint delay_) public{
        admin = msg.sender;
        timelockImplementation = initialTimelockImplementation;
        delay = delay_;
    }


    function () payable external {
        // delegate all other functions to current implementation
        (bool success, ) = timelockImplementation.delegatecall(msg.data);

        assembly {
                let free_mem_ptr := mload(0x40)
                returndatacopy(free_mem_ptr, 0, returndatasize)

                switch success
                case 0 { revert(free_mem_ptr, returndatasize) }
                default { return(free_mem_ptr, returndatasize) }
        }
    }
    
}