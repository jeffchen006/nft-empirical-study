contract Trex {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event receivedNative(uint256 amount);

    function receiveNative(address to) payable public {
        uint256 amount = msg.value;
        require(amount > 0, "You need to send some amount");
        payable(to).transfer(amount);
        emit receivedNative(amount);
    }
}