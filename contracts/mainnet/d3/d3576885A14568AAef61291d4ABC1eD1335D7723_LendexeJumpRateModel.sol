// File: contracts/LendexeJumpRateModel.sol

pragma solidity ^0.5.16;



/**
 * @title Lendexe's JumpRateModel Contract
 * @author Fujin
 */
contract LendexeJumpRateModel is InterestRateModel {
    using SafeMath for uint256;
    using SignedSafeMath for int256;

    event NewInterestParams(
        uint256 optimalRatePerBlock,
        uint256 jumpRatePerBlock,
        uint256 kink
    );
    uint256 public constant blocksPerYear = 2102400;
    uint256 public optimalRatePerBlock;
    uint256 public jumpRatePerBlock;
    uint256 public kink;

    constructor(
        uint256 optimalRatePerYear,
        uint256 jumpRatePerYear,
        uint256 kink_
    ) public {
        optimalRatePerBlock = optimalRatePerYear.div(blocksPerYear);
        jumpRatePerBlock = jumpRatePerYear.div(blocksPerYear);
        kink = kink_;

        emit NewInterestParams(optimalRatePerBlock, jumpRatePerBlock, kink);
    }

    function utilizationRate(
        uint256 cash,
        uint256 borrows,
        uint256 reserves
    ) public pure returns (uint256) {
        // Utilization rate is 0 when there are no borrows
        if (borrows == 0) {
            return 0;
        }
        return borrows.mul(1e18).div(cash.add(borrows));
    }

    function getBorrowRate(
        uint256 cash,
        uint256 borrows,
        uint256 reserves
    ) public view returns (uint256) {
        uint256 util = utilizationRate(cash, borrows, reserves);
        int256 _optimalRatePerBlock = int256(optimalRatePerBlock);
        int256 _kink = int256(kink);
        if (util <= kink.div(100)) {
            uint256 B = optimalRatePerBlock.mul(1e18).div(kink);
            return B.mul(util).div(1e18);
        } else {
            
            int256 _jumpRatePerBlock = int256(jumpRatePerBlock);
            int256 _blocksPerYear = int256(blocksPerYear);

            int256 jumpRate = _optimalRatePerBlock.sub(_jumpRatePerBlock).mul(_blocksPerYear);
                 
            int256 B = jumpRate.mul(1e18).div(int256(_kink).sub(100e18));
            int256 A = _optimalRatePerBlock.mul(_blocksPerYear).sub(B.mul(_kink).div(1e18));
            return uint256(A.add(B.mul(100).mul(int256(util)).div(1e18)).div(_blocksPerYear).div(100));
        }
    }

    function getSupplyRate(
        uint256 cash,
        uint256 borrows,
        uint256 reserves,
        uint256 reserveFactorMantissa
    ) public view returns (uint256) {
        uint256 oneMinusReserveFactor = uint256(1e18).sub(
            reserveFactorMantissa
        );
        uint256 borrowRate = getBorrowRate(cash, borrows, reserves);
        uint256 rateToPool = borrowRate.mul(80).mul(oneMinusReserveFactor).div(
            1e18
        );
        return
            utilizationRate(cash, borrows, reserves).mul(rateToPool).div(1e20);
    }
}