        }
    }

    /**
     * @dev Hook that is called before a set of serially-ordered token ids are about to be transferred. This includes minting.
     * And also called before burning one token.
     *
     * startTokenId - the first token id to be transferred
     * quantity - the amount to be transferred
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, `from`'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, `tokenId` will be burned by `from`.
     * - `from` and `to` are never both zero.
     */
    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal virtual {}

    /**
     * @dev Hook that is called after a set of serially-ordered token ids have been transferred. This includes
     * minting.
     * And also called after one token has been burned.
     *
     * startTokenId - the first token id to be transferred
     * quantity - the amount to be transferred
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, `from`'s `tokenId` has been
     * transferred to `to`.
     * - When `from` is zero, `tokenId` has been minted for `to`.
     * - When `to` is zero, `tokenId` has been burned by `from`.
     * - `from` and `to` are never both zero.
     */
    function _afterTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal virtual {}
}

interface IERC20 {
    function balanceOf(address _owner) external returns (uint256);
    function transfer(address recipient, uint256 amount) external returns (bool);
}

contract KBK is ERC721A, Ownable {
    
    using Strings for uint256;

    mapping (uint256 => string) _tokenUri;

    //supply id -> category
    mapping (uint256 => uint256) private _dedicatedId;
    mapping (uint256 => bool) public validIds;

    mapping (address => bool) public whitelisted;

    modifier onlyWhitelisted() {
        require(whitelisted[msg.sender],"Error: Caller Must be Whitelisted!!");
        _;
    }

    constructor() ERC721A("KICK BACK KOALAS", "KBK") {
        validIds[1] = true;
        validIds[2] = true;
    }
    
    //Single Mint
    function mint(address _account, uint256 _pid) external onlyOwner {
        IdChecker(_pid);
        _safeMint(_account, 1);
        uint256 currentSupply = totalSupply();
        _dedicatedId[currentSupply] = _pid;
    }

    function AirDrop(address[] calldata _address, uint256[] calldata _pid) external onlyOwner {

        require(_address.length == _pid.length,"Error: Length Mismatch!!");

        for(uint i = 0; i < _address.length; i++) {
            IdChecker(_pid[i]);
            _safeMint(_address[i], 1);
            uint256 currentSupply = totalSupply();
            _dedicatedId[currentSupply] = _pid[i];
        }
    }

    function rewardMint(address _account,uint _pid) external onlyWhitelisted {
        IdChecker(_pid);
        _safeMint(_account, 1);
        uint256 currentSupply = totalSupply();
        _dedicatedId[currentSupply] = _pid;
    }

    function setWhitelisted(address _adr, bool _status) external onlyOwner {
        whitelisted[_adr] = _status;
    }

    function contractURI() public pure returns (string memory) {
        return "https://gateway.pinata.cloud/ipfs/QmZ6wzvo1wU7UwyGg9Q3F5zuhzcAmJPyodYg8NBJi1UC3x/";
    }

    function IdChecker(uint _id) view internal {
        if(!validIds[_id]) {
            revert("ERROR: ID NOT VALID!!");
        }
    }

    function setValidation(uint _id, bool _status) external onlyOwner {
        validIds[_id] = _status;
    }

    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }

    //token ids represent the two nfts 1 and 2 
    function setTokenUri(uint _id, string calldata _uri) external onlyOwner {
        IdChecker(_id);
        _tokenUri[_id] = _uri;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(
        _exists(tokenId),
        "ERC721Metadata: URI query for nonexistent token"
        );

        uint _rId =  _dedicatedId[tokenId];
        return _tokenUri[_rId];
    }

    function walletOfOwner(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory ownedTokenIds = new uint256[](ownerTokenCount);
        uint256 currentTokenId = 1;
        uint256 ownedTokenIndex = 0;

        while (ownedTokenIndex < ownerTokenCount && currentTokenId <= totalSupply()) {
            address currentTokenOwner = ownerOf(currentTokenId);

            if (currentTokenOwner == _owner) {
                ownedTokenIds[ownedTokenIndex] = currentTokenId;

                ownedTokenIndex++;
            }
            currentTokenId++;
        }

        return ownedTokenIds;
    }

    function withdraw() external onlyOwner {
        (bool os, ) = payable(owner()).call{value: address(this).balance}("");
        require(os);
    }

    function rescueStuckedToken(address _token) external onlyOwner {
        uint balance = IERC20(_token).balanceOf(address(this));
        IERC20(_token).transfer(owner(),balance);
    }

}