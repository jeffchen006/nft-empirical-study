contract OMNINFTLogic is ERC721ABurnableUpgradeable, ERC1967AdminStorage {
    using OmniNFTStorage for OmniNFTStorage.Layout;
    using ECDSA for bytes32;

    event SaleChanged(
        uint64 saleStartTime,
        uint64 saleEndTime,
        uint256 salePrice,
        uint32 saleLimit
    );

    event SyntheticMinted(uint256[] burnedToken, uint256 mintedToken);

    // Take note of the initializer modifiers.
    // - `initializerERC721A` for `ERC721AUpgradeable`.
    function initialize(
        string memory name, // NFT name
        string memory symbol, // NFT symbol
        string memory baseURI, // NFT baseURI
        uint32 maxSupply, // NFT maxSupply,once set can't be changed
        address payable paymentreceiver, // EOA or ContractWallet,recieves payment from mints
        address adminRole, // EOA or ContractWallet,contract controller（admin role）
        address saleRole, // EOA or ContractWallet,can set sale price,sale time
        address mintRole, // EOA only, can sign for special mint
        address synthRole, // EOA only, can sign for NFT synthesis
        address defaultApproval // default approval address ( token stake contract)
    ) external initializerERC721A {
        __ERC721A_init(name, symbol);

        OmniNFTStorage.Layout storage l = OmniNFTStorage.layout();

        l.baseURI = baseURI;
        l.mintLimit = maxSupply;
        l.paymentReceiver = paymentreceiver;

        require(adminRole != address(0), "owner is the zero address");

        _setAdmin(adminRole);

        l.SALE_ROLE = saleRole;
        l.MINT_ROLE = mintRole;
        l.SYNTH_ROLE = synthRole;

        l.defalutApproval = defaultApproval;
    }

    function saleInfo()
        external
        view
        returns (
            uint64 saleStartTime,
            uint64 saleEndTime,
            uint256 salePrice,
            uint32 remainingSaleCount
        )
    {
        OmniNFTStorage.Layout storage l = OmniNFTStorage.layout();
        saleStartTime = l.saleStartTime;
        saleEndTime = l.saleEndTime;
        salePrice = l.salePrice;
        remainingSaleCount = l.remainingSaleCount;
    }

    function mintedSignature(bytes32 signature) external view returns (bool) {
        return OmniNFTStorage.layout().mintedSignature[signature];
    }

    function owner() public view returns (address) {
        return _getAdmin();
    }

    function SALE_ROLE() public view returns (address) {
        return OmniNFTStorage.layout().SALE_ROLE;
    }

    function MINT_ROLE() public view returns (address) {
        return OmniNFTStorage.layout().MINT_ROLE;
    }

    function SYNTH_ROLE() public view returns (address) {
        return OmniNFTStorage.layout().SYNTH_ROLE;
    }

    function paymentReceiver() public view returns (address payable) {
        return OmniNFTStorage.layout().paymentReceiver;
    }

    function defalutApproval() public view returns (address) {
        return OmniNFTStorage.layout().defalutApproval;
    }

    modifier onlyOwner() {
        require(msg.sender == owner(), "caller is not the owner");
        _;
    }
    modifier onlySaleRole() {
        require(msg.sender == SALE_ROLE(), "caller is not sale role");
        _;
    }

    function checkMintRole(address account) private view {
        require(
            account == MINT_ROLE(),
            string(
                abi.encodePacked(
                    Strings.toHexString(uint256(uint160(account))),
                    " is not mint role"
                )
            )
        );
    }

    function checkSynthRole(address account) private view {
        require(
            account == SYNTH_ROLE(),
            string(
                abi.encodePacked(
                    Strings.toHexString(uint256(uint160(account))),
                    " is not synth role"
                )
            )
        );
    }

    function transferOwnership(address newOwner) public onlyOwner {
        require(newOwner != address(0), "new owner is the zero address");
        _setAdmin(newOwner);
    }

    function setSaleRole(address newSaleRole) public onlyOwner {
        OmniNFTStorage.layout().SALE_ROLE = newSaleRole;
    }

    function setMintRole(address newMintRole) public onlyOwner {
        OmniNFTStorage.layout().MINT_ROLE = newMintRole;
    }

    function setSynthRole(address newSynthRole) public onlyOwner {
        OmniNFTStorage.layout().SYNTH_ROLE = newSynthRole;
    }

    /**
     * @dev Set the base URI for all token IDs, only admin can call
     * @param newBaseURI New base URI (RFC 3986)
     */
    function updateBaseURI(string memory newBaseURI) public onlyOwner {
        OmniNFTStorage.layout().baseURI = newBaseURI;
    }

    function _baseURI() internal view override returns (string memory) {
        return OmniNFTStorage.layout().baseURI;
    }

    /**
     * @dev Returns if the `_operator` is allowed to manage all of the assets of `_owner`
     *
     * See {setApprovalForAll}.
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override(ERC721AUpgradeable, IERC721AUpgradeable)
        returns (bool)
    {
        return
            _operator == defalutApproval() ||
            super.isApprovedForAll(_owner, _operator);
    }

    /**
     * @dev Set address who recieves payment from mints
     */
    function setPaymentReceiver(address payable account) public onlyOwner {
        require(account != address(0), "Cannot set zero address");
        OmniNFTStorage.layout().paymentReceiver = account;
    }

    /**
     * @dev Set sale price, sale start time, sale end time, sale limit
     * @param saleStartTime sale start time
     * @param saleEndTime sale end time
     * @param salePrice sale price
     * @param saleLimit sale limit
     */
    function updateSale(
        uint64 saleStartTime,
        uint64 saleEndTime,
        uint256 salePrice,
        uint32 saleLimit
    ) public onlySaleRole {
        require(saleEndTime >= saleStartTime);
        OmniNFTStorage.Layout storage l = OmniNFTStorage.layout();
        l.saleStartTime = saleStartTime;
        l.saleEndTime = saleEndTime;
        l.salePrice = salePrice;
        l.remainingSaleCount = saleLimit;
        emit SaleChanged(saleStartTime, saleEndTime, salePrice, saleLimit);
    }

    /**
     * @dev Get price
     */
    function getPrice(uint256 amount) private view returns (uint256) {
        uint256 price = OmniNFTStorage.layout().salePrice;
        require(price > 0, "Sale price not set");
        return amount * price;
    }

    /**
     * @dev batch transfers the ownership of an NFT from one address to another address
     */
    function safeBatchTransfer(address to, uint256[] memory tokenIds) external {
        safeBatchTransferFrom(msg.sender, to, tokenIds);
    }

    /**
     * @dev batch Transfer ownership of an NFT -- THE CALLER IS RESPONSIBLE
     *  TO CONFIRM THAT `_to` IS CAPABLE OF RECEIVING NFTS OR ELSE
     *  THEY MAY BE PERMANENTLY LOST
     */
    function batchTransfer(address to, uint256[] memory tokenIds) external {
        batchTransferFrom(msg.sender, to, tokenIds);
    }

    /**
     * @dev batch transfers the ownership of an NFT from one address to another address
     */
    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory tokenIds
    ) public {
        for (uint256 i = 0; i < tokenIds.length; ) {
            safeTransferFrom(from, to, tokenIds[i]);
            unchecked {
                i++;
            }
        }
    }

    /**
     * @dev batch Transfer ownership of an NFT -- THE CALLER IS RESPONSIBLE
     *  TO CONFIRM THAT `_to` IS CAPABLE OF RECEIVING NFTS OR ELSE
     *  THEY MAY BE PERMANENTLY LOST
     */
    function batchTransferFrom(
        address from,
        address to,
        uint256[] memory tokenIds
    ) public {
        for (uint256 i = 0; i < tokenIds.length; ) {
            transferFrom(from, to, tokenIds[i]);
            unchecked {
                i++;
            }
        }
    }

    /**
     * @dev modifier of sale activity
     * @param to to address
     * @param quantity Number of NFTs to mint
     */
    modifier forSale(address to, uint32 quantity) {
        //require(amount > 0, "Cannot mint zero"); // has already checked in _safeMint
        OmniNFTStorage.Layout storage l = OmniNFTStorage.layout();
        require(l.mintLimit >= _totalMinted() + quantity, "Insufficent supply");

        uint256 tsNow = block.timestamp;
        require(
            l.saleStartTime < tsNow && l.saleEndTime > tsNow,
            "Sale not open"
        );
        require(l.remainingSaleCount >= quantity, "Sale limit reached");
        unchecked {
            l.remainingSaleCount -= quantity;
        }

        _;

        _safeMint(to, quantity);
    }

    /**
     * @dev Batch mint consecutive NFTs to `msg.sender` wallet
     * @notice ETH will not be automatically sent back to `msg.sender` when fail, Please contact customer service to refund
     * @param quantity Number of NFTs to mint
     */
    function batchMint(uint32 quantity) external payable {
        batchMintTo(msg.sender, quantity);
    }

    /**
     * @dev Batch mint consecutive NFTs to `to` wallet
     * @notice ETH will not be automatically sent back to `msg.sender` when fail, Please contact customer service to refund
     * @param to to address
     * @param quantity Number of NFTs to mint
     */
    function batchMintTo(address to, uint32 quantity)
        public
        payable
        forSale(to, quantity)
    {
        uint256 price = getPrice(quantity);
        require(msg.value >= price, "Insufficent input amount");
    }

    /**
     * @dev Mint NFT using signed message
     * @notice ETH will not be automatically sent back to `msg.sender` when fail, Please contact customer service to refund
     * @param quantity Number of NFTs to mint
     * @param nonce nonce (prevent replay attacks)
     * @param deadline deadline 
     * @param signature signed message(`to`+`msg.value`+`amount_`+`nonce`) by mint role
     */
    function signedBatchMint(
        uint32 quantity,
        uint32 nonce,
        uint64 deadline,
        bytes calldata signature
    ) external payable {
        signedBatchMintTo(msg.sender, quantity, nonce, deadline, signature);
    }

    /**
     * @dev Mint NFT using signed message
     * @notice ETH will not be automatically sent back to `msg.sender` when fail, Please contact customer service to refund
     * @param to to address
     * @param quantity Number of NFTs to mint
     * @param nonce nonce (prevent replay attacks)
     * @param deadline deadline 
     * @param signature signed message(`to`+`msg.value`+`amount_`+`nonce`) by mint role
     */
    function signedBatchMintTo(
        address to,
        uint32 quantity,
        uint32 nonce,
        uint64 deadline,
        bytes calldata signature
    ) public payable forSale(to, quantity) {
       require(block.timestamp <= deadline, "expired");
        bytes32 _hash = keccak256(
            abi.encodePacked(to, msg.value, quantity, nonce, deadline, address(this))
        );
        checkMintRole(_hash.toEthSignedMessageHash().recover(signature));
        OmniNFTStorage.Layout storage l = OmniNFTStorage.layout();
        require(!l.mintedSignature[_hash], "Already minted");
        l.mintedSignature[_hash] = true;
    }

    /**
     * @dev Synthesized through NFTs
     * @param ids NFT from
     * @param signature signed message(`ids`+`address(this)`) by synth role
     */
    function SyntheticMint(uint256[] calldata ids, bytes calldata signature)
        external
    {
        uint32 len = uint32(ids.length);
        require(len > 1, "Cannot synthetically mint less than 2 NFTs");
        bytes32 _hash = keccak256(abi.encodePacked(ids, address(this))); // add address(this) to prevent replay attacks

        checkSynthRole(_hash.toEthSignedMessageHash().recover(signature));

        for (uint256 i = 0; i < len; ) {
            burn(ids[i]);
            unchecked {
                i++;
            }
        }
        unchecked {
            OmniNFTStorage.layout().mintLimit += 1;
        }
        uint256 nextTokenID = _nextTokenId();
        _safeMint(msg.sender, 1);
        emit SyntheticMinted(ids, nextTokenID);
    }

    /**
     * @dev Admin withdraw function
     */
    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, "No balance to withdraw");
        Address.sendValue(paymentReceiver(), balance);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721AUpgradeable, IERC721AUpgradeable)
        returns (bool)
    {
        return ERC721AUpgradeable.supportsInterface(interfaceId);
    }

    function tokenURI(uint256 tokenId) public view override(ERC721AUpgradeable, IERC721AUpgradeable) returns (string memory) {
        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();
        string memory baseURI = _baseURI();
        return bytes(baseURI).length != 0 ? string(abi.encodePacked(baseURI, _toString(tokenId), ".json")) : '';
    }

    function setDefaultApproval(address _defaultApproval) external onlyOwner {
        OmniNFTStorage.Layout storage l = OmniNFTStorage.layout();
        l.defalutApproval = _defaultApproval;
    }
}