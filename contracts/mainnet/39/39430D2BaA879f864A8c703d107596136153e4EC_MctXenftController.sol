library MintCoder {
    function toU256(bool x) internal pure returns (uint256 r) {
        assembly {
            r := x
        }
    }

    function encodeMintInfo(
        uint256 term,
        uint256 maturityTs,
        uint256 rank,
        uint256 amp,
        uint256 eaa,
        bool emptySlot
    ) internal pure returns (uint256 info) {
        info = info | toU256(emptySlot);
        info = info | (eaa << 16);
        info = info | (amp << 32);
        info = info | (rank << 48);
        info = info | (maturityTs << 176);
        info = info | (term << 240);
    }

    function decodeMintInfo(uint256 info)
        internal
        pure
        returns (
            uint256 term,
            uint256 maturityTs,
            uint256 rank,
            uint256 amp,
            uint256 eaa,
            bool emptySlot
        )
    {
        term = uint16(info >> 240);
        maturityTs = uint64(info >> 176);
        rank = uint128(info >> 48);
        amp = uint16(info >> 32);
        eaa = uint16(info >> 16);
        emptySlot = uint16(info) == 1;
    }
}



