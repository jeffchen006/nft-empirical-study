                    granterCopyright.excuteFlag &= ~grantMask;
                    granteeCopyright.excuteFlag |= grantMask;
                }
            }
        }
        _addOrUpdateHolderWithHash(hash, granterCopyright);
        _addOrUpdateHolderWithHash(hash, granteeCopyright);
    }

