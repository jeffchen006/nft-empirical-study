    // Owner functionality ------------------------------------------------------------------------
    /**
     * @notice Sets the burn contract address of the collection.
     * @param contractToSet The uri to set.
     */
    function setBurnContract(address contractToSet) external onlyOwner {
        boboAddress = contractToSet;
        boboContract = BiPlaneBobo(boboAddress);
    }

    /**
     * @notice Sets the amount of points required to burn.
     * @param pointsRequired The points required to set.
     */
    function setPointsRequiredToBurn(uint16 pointsRequired) external onlyOwner {
        pointsRequiredToBurn = pointsRequired;
    }

    /**
     * @notice Sets the amount burns available.
     * @param maxBurn The max amount of burns available.
     */
    function setMaxBurns(uint8 maxBurn) external onlyOwner {
        maxTotalBurn = maxBurn;
    }

    /**
     * @notice Sets the address to burn to.
     * @param burnAddress The new address.
     */
    function setBurnAddress(address burnAddress) external onlyOwner {
        targetBurnAddress = burnAddress;
    }

    /**
     * @notice Turns off burn
     */
    function setBurnOff() external onlyOwner {
        burnState = BurnState.OFF;
    }

    /**
     * @notice Turns on burn
     */
    function setBurnOn() external onlyOwner {
        burnState = BurnState.ON;
    }

    /**
     * @notice Sets burn complete (functionally equivalent to OFF)
     */
    function setBurnComplete() external onlyOwner {
        burnState = BurnState.COMPLETE;
    }

    /**
     * @notice Gets balanceOf studio badge for a given address.
     * @param ownerAddress The new address.
     */
    function balanceOf(address ownerAddress) public view returns (uint256) {
        return hasBurned[ownerAddress] ? 1 : 0;
    }

    // Burn functionality ------------------------------------------------------------------------
    /**
     * @notice Gets the point total for a given array of tokenids
     * @param tokenIdsToBurn The array of tokenIds to validate
     */
    function getPointTotal(uint256[] calldata tokenIdsToBurn)
        public
        view
        returns (uint16)
    {
        uint16 points;
        for (uint16 i = 0; i < tokenIdsToBurn.length; i++) {
            points += tokenIdPointValues[tokenIdsToBurn[i]];
        }
        return points;
    }

    /**
     * @notice Validates a burn is available, the minimum point requirement is met, and user has not already burned. Then burns all tokenIds in the array.
     * @dev Take note that msg.sender must own all tokenIds and must have called setApprovalForAll to give this contract permission to burn.
     * @param tokenIdsToBurn The array of tokenIds to validate.
     */
    function burn(uint256[] calldata tokenIdsToBurn) public {
        if (burnState != BurnState.ON) revert BurnNotOn();
        uint16 pointTotal = getPointTotal(tokenIdsToBurn);
        if (pointTotal < pointsRequiredToBurn) revert NotEnoughPoints();
        // Burning a gold bobo is worth 2 badges.
        // The only feasible way to burn 10k points is by burning a gold bobo.
        // In reality they are worth 500 points, but for this contract's functionality, they are set to 10k.
        if (pointTotal >= 10000 && burnCounter + 2 > maxTotalBurn)
            revert NoBurnsLeft();
        if (burnCounter + 1 > maxTotalBurn) revert NoBurnsLeft();
        if (hasBurned[msg.sender]) revert BurnedAlready();
        for (uint16 i = 0; i < tokenIdsToBurn.length; i++) {
            boboContract.transferFrom(
                msg.sender,
                targetBurnAddress,
                tokenIdsToBurn[i]
            );
        }
        emit ThankYouFromBoboAndTeam();
        emit YourStudioBadgeWillArriveSoon();
        hasBurned[msg.sender] = true;
        burnCounter++;
        if (pointTotal >= 10000) {
            // Burning a gold bobo is worth 2 badges
            burnCounter++;
            burnedGold[msg.sender] = true;
        }
    }
