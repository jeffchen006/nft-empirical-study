abstract contract DerivedERC2981Royalty is IERC2981 {

    event RoyaltyUpdated(uint256 value);

    uint256 private _royalty;

    function _setRoyalty(uint256 value ) internal {
        require(value <= 10000, "Royalty more that 100%" );
