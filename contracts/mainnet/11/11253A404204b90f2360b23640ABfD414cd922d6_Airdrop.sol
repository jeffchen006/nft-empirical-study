contract Airdrop is Ownable{

    address nftContract = 0xa342f5D851E866E18ff98F351f2c6637f4478dB5;

    function airdrop(uint256 ID ,uint256[] calldata amounts, address[] calldata users) public onlyOwner {
        require(amounts.length == users.length, "Length of both arrays are not the same");
        for(uint256 i=0; i < users.length; i++){
            IERC1155(nftContract).safeTransferFrom(msg.sender, users[i], ID, amounts[i], "");
        }
    }

    function transferEthToOwner() public onlyOwner  {
        require(address(this).balance > 0, "Balance is 0");
        payable(owner()).transfer(address(this).balance);
    }


    function changeNFTAddress(address newaddress) public onlyOwner{
        nftContract = newaddress;
    }

    receive() external payable {}

    function addEth() public payable {}

}