interface IUnamano {
    function poolLength() external view returns (uint256);
    function quitAll(uint256 _pid, uint256 _index, uint256 _length) external;
    function giveUpAll(uint256 _pid, uint256 _index, uint256 _length) external;
    function transferProjectCandyAsset(uint256 _pid) external;
    function setProjectStatus(uint256 _pid, uint8 _status) external;
}

interface IUnamanoToken {
    function editFeeAddress(address _feeAddress) external;
}

contract MultisignatureWallet is Ownable,ReentrancyGuard {

    using SafeMath for uint256;
    using BytesLib for bytes;

    // txKey for multisignature functions
    mapping(string => uint8) public completedTxs;
    mapping(address => uint8) public managers;
    address[] public managerArray;
    // Current wallet version
    uint8 constant VERSION = 0;
    // hash calculation with salt
    uint public hashSalt; 
    // Minimum number of signatures for current multisig
    uint8 public current_min_signatures;
    address public operatorAddress;

    event NewOperatorAddress(address operator);
    event ProjectrQuitAll(uint256 indexed pid, string txKey);
    event ProjectrGiveUpAll(uint256 indexed pid, string txKey);
    event ProjectTransferCandyAsset(uint256 indexed pid, string txKey);
    event ProjectResume(uint256 indexed pid, string txKey);
    event ProjectPause(uint256 indexed pid, string txKey);
    event UnamanoTokenFeeAddress(address feeAddress, string txKey);

    modifier onlyOperator() {
        require(msg.sender == operatorAddress, "Not operator");
        _;
    }

    constructor(
        uint256 _chainid, 
        address _operatorAddress,
        address[] memory _managers
    ) {
        require(_managers.length == 5, "Error number of managers");
        managerArray = _managers;
        for (uint8 i = 0; i < managerArray.length; i++) {
            managers[managerArray[i]] = 1;
        }
        // Set the minimum number of signatures for the current transaction
        current_min_signatures = 3;
        hashSalt = _chainid * 2 + VERSION;
        operatorAddress = _operatorAddress;
    }

    // Exit staking for all users
    function quitAll(address poolAddress, uint256 _pid, uint256 _index, uint256 _length, string memory txKey, bytes memory signatures) external nonReentrant onlyOperator {
        IUnamano UNA = IUnamano(poolAddress);
        require(_pid < UNA.poolLength(), "invalid pool id");
        require(completedTxs[txKey] == 0, "Transaction has been completed");
        bytes32 vHash = keccak256(abi.encodePacked(txKey, _pid, _index, _length, bytes("quitAll"), hashSalt));
        // check signature
        require(validSignature(vHash, signatures), "Valid signatures fail");
        completedTxs[txKey] = 1;
        UNA.quitAll(_pid, _index, _length);
        emit ProjectrQuitAll(_pid, txKey);
    }

    // Exit all users' staking and forfeit rewards
    function giveUpAll(address poolAddress, uint256 _pid, uint256 _index, uint256 _length, string memory txKey, bytes memory signatures) external nonReentrant onlyOperator {
        IUnamano UNA = IUnamano(poolAddress);
        require(_pid < UNA.poolLength(), "invalid pool id");
        require(completedTxs[txKey] == 0, "Transaction has been completed");
        bytes32 vHash = keccak256(abi.encodePacked(txKey, _pid, _index, _length, bytes("giveUpAll"), hashSalt));
        // check signature
        require(validSignature(vHash, signatures), "Valid signatures fail");
        completedTxs[txKey] = 1;
        UNA.giveUpAll(_pid, _index, _length);
        emit ProjectrGiveUpAll(_pid, txKey);
    }

    // Transfer out project token to the project creator
    function transferProjectCandyAsset(address poolAddress, uint256 _pid, string memory txKey, bytes memory signatures) external nonReentrant onlyOperator {
        IUnamano UNA = IUnamano(poolAddress);
        require(_pid < UNA.poolLength(), "invalid pool id");
        require(completedTxs[txKey] == 0, "Transaction has been completed");
        bytes32 vHash = keccak256(abi.encodePacked(txKey, _pid, bytes("transferProjectCandyAsset"), hashSalt));
        // check signature
        require(validSignature(vHash, signatures), "Valid signatures fail");
        completedTxs[txKey] = 1;
        UNA.transferProjectCandyAsset(_pid);
        emit ProjectTransferCandyAsset(_pid, txKey);
    }

    // Pause and resume projects
    function setProjectStatus(address poolAddress, uint256 _pid, string memory txKey, uint8 _status, bytes memory signatures) external nonReentrant onlyOperator {
        require(_status == 1 || _status == 2, "invalid status");
        IUnamano UNA = IUnamano(poolAddress);
        require(_pid < UNA.poolLength(), "invalid pool id");
        require(completedTxs[txKey] == 0, "Transaction has been completed");
        bytes32 vHash = keccak256(abi.encodePacked(txKey, _pid, bytes("setProjectStatus"), _status, hashSalt));
        // check signature
        require(validSignature(vHash, signatures), "Valid signatures fail");
        completedTxs[txKey] = 1;
        UNA.setProjectStatus(_pid, _status);
        if (_status == 2) {
            emit ProjectPause(_pid, txKey);
        } else {
            emit ProjectResume(_pid, txKey);
        }
    }

    // edit fee address in the unamano token
    function editFeeAddress(address _tokenAddress, address _feeAddress, string memory txKey, bytes memory signatures) external nonReentrant onlyOperator {
        IUnamanoToken UNA = IUnamanoToken(_tokenAddress);
        require(completedTxs[txKey] == 0, "Transaction has been completed");
        bytes32 vHash = keccak256(abi.encodePacked(_feeAddress, txKey, bytes("editFeeAddress"), hashSalt));
        // check signature
        require(validSignature(vHash, signatures), "Valid signatures fail");
        completedTxs[txKey] = 1;
        UNA.editFeeAddress(_feeAddress);
        emit UnamanoTokenFeeAddress(_feeAddress, txKey);
    }

    function setOperator(address _operatorAddress) external onlyOwner {
        require(_operatorAddress != address(0), "Cannot be zero address");
        operatorAddress = _operatorAddress;

        emit NewOperatorAddress(_operatorAddress);
    }

    function validSignature(bytes32 hash, bytes memory signatures) internal view returns (bool) {
        require(signatures.length <= 325, "Max length of signatures: 325");
        // Get the valid administrators corresponding to the signature list, if there is an incorrect signature or not the administrator's signature, it will fail
        bytes32 ethSignedMessageHash = ECDSA.toEthSignedMessageHash(hash);
        uint sManagersCount = getManagerFromSignatures(ethSignedMessageHash, signatures);
        // Check the minimum number of signatures
        return sManagersCount >= current_min_signatures;
    }

    function getManagerFromSignatures(bytes32 hash, bytes memory signatures) internal view returns (uint){
        uint signCount = 0;
        uint times = signatures.length.div(65);
        address[] memory result = new address[](times);
        uint k = 0;
        uint8 j = 0;
        for (uint i = 0; i < times; i++) {
            bytes memory sign = signatures.slice(k, 65);
            address mAddress = ECDSA.recover(hash, sign);
            require(mAddress != address(0), "Signatures error");
            // counter
            if (managers[mAddress] == 1) {
                signCount++;
                result[j++] = mAddress;
            }
            k += 65;
        }
        // Check for address duplication
        bool suc = repeatability(result);
        delete result;
        require(suc, "Signatures duplicate");
        return signCount;
    }

    function repeatability(address[] memory list) internal pure returns (bool) {
        for (uint i = 0; i < list.length; i++) {
            address address1 = list[i];
            if (address1 == address(0)) {
                break;
            }
            for (uint j = i + 1; j < list.length; j++) {
                address address2 = list[j];
                if (address2 == address(0)) {
                    break;
                }
                if (address1 == address2) {
                    return false;
                }
            }
        }
        return true;
    }
}