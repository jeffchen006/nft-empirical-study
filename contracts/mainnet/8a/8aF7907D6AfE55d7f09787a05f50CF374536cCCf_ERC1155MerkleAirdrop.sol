// File contracts/TestERC1155.sol

pragma solidity ^0.8.0;



contract ERC1155MerkleAirdrop is ERC1155, Ownable {
    bytes32 public immutable merkleRoot =  0x0646b85db02703a8916b0e868103457eae9b8a946528d759569d4b45af0954ce;
    uint256 public immutable startAt = 1673536067540;
    uint256 public immutable endAt = 1673536067540;
    uint256 public immutable airdropID = 2;
    uint256 public immutable totalTokens = 0x1;
    uint256 public claimedTokens = 0;
    uint256 public availableTokens = 1;

    mapping(address => bool) public claimed;

    event Claimed(address account, uint256 amount);

    constructor() ERC1155("AlphaTEST") {
        _setURI("https://gateway.pinata.cloud/ipfs/QmYzoGWU5Lhg8ZF1tB7Z6Gh1SLnYqFQoHNKyC83PyAT9sK");
    }

    function safeMint(address to, uint256 amount) private {
        _mint(to, 1, amount, "");
    }

    function claim(
        address account,
        uint256 amount,
        bytes32[] calldata merkleProof
    ) public {
        require(claimed[account] == false, "user already claimed");

        bytes32 node = keccak256(abi.encodePacked(account, amount));

        require(
            MerkleProof.verify(merkleProof, merkleRoot, node),
            "invalid proof"
        );

        claimed[account] = true;
        claimedTokens += amount;
        availableTokens -= amount;
        safeMint(account, amount);

        emit Claimed(account, amount);
    }

    function checkTime() private view {
        require(endAt == 0 || endAt >= block.timestamp, "airdrop already expired");
        require(startAt == 0 || startAt <= block.timestamp, "airdrop not started yet");
    }
}