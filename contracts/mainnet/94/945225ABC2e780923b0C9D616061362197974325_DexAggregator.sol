contract DexAggregator is Initializable, OwnableUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    //Uniswap v3 quoterv2 reference
    IQuoterV2 public QOUTERV2;
    //Uniswap v3 router reference
    ISwapRouter02 public SWAP_ROUTER_02;
    //Uniswap v2 router reference
    IUniswapV2Router02 public UNIV2_SWAP_ROUTER;
    //Sushiswap router reference
    IUniswapV2Router02 public SUSHI_SWAP_ROUTER;
    //Uniswap v2 factory reference
    IUniswapV2Factory public UNIV2_FACTORY;
    //Sushiswap router reference
    IUniswapV2Factory public SUSHI_FACTORY;
    //Uniswap v3 factory reference
    IUniswapV3Factory public UNIV3_FACTORY;

    ///@notice constructor for the dex aggregator
    ///@param _sushifactory The factory address of sushiswap
    ///@param _sushirouter The router address of suhsiswap
    function initialize(
        address _sushifactory,
        address _sushirouter
    ) public initializer {
        SUSHI_SWAP_ROUTER = IUniswapV2Router02(_sushirouter);
        SUSHI_FACTORY = IUniswapV2Factory(_sushifactory);
        UNIV3_FACTORY = IUniswapV3Factory(
            0x1F98431c8aD98523631AE4a59f267346ea31F984
        );
        UNIV2_FACTORY = IUniswapV2Factory(
            0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f
        );
        UNIV2_SWAP_ROUTER = IUniswapV2Router02(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );

        SWAP_ROUTER_02 = ISwapRouter02(
            0x68b3465833fb72A70ecDF485E0e4C7bD8665Fc45
        );

        QOUTERV2 = IQuoterV2(0x61fFE014bA17989E743c5F6cB21bF9697530B21e);
    }

    ///@notice Creates byte path for uniswap v3 quoter rate function with 0.03% fee
    ///@param path the path of addresses that we want to check swap in
    function convertToBytesPath(
        address[] memory path
    ) private pure returns (bytes memory bytesPath) {
        uint256 i;
        for (i = 0; i < path.length - 1; i++) {
            bytesPath = abi.encodePacked(bytesPath, path[i], uint24(3000));
        }
        bytesPath = abi.encodePacked(bytesPath, path[i]);
    }

    ///@notice returns the uniswap v3 (0.03% pool) amount out for given path address and amountIn
    ///@param pathaddress the path of addresses that we want to check swap in
    ///@param amountIn the amount of the token that we want to swap for
    function getuniswapv3amount(
        address[] memory pathaddress,
        uint amountIn
    ) public returns (uint) {
        bytes memory path = convertToBytesPath(pathaddress);
        try QOUTERV2.quoteExactInput(path, amountIn) returns (
            uint256 amountOut,
            uint160[] memory sqrtPriceX96AfterList,
            uint32[] memory initializedTicksCrossedList,
            uint256 gasEstimate
        ) {
            return (amountOut);
        } catch Error(string memory /*reason*/) {
            return 0;
        } catch Panic(uint) {
            return 0;
        } catch (bytes memory) {
            return 0;
        }
    }

    ///@notice checks if the pool exist between token weth and tokenout and weth
    ///@param tokenIn The address of the token in
    ///@param tokenOut The address of the token out
    function poolcheck(
        address tokenIn,
        address tokenOut
    ) public view returns (bool) {
        address[] memory path = new address[](3);
        path[0] = tokenIn;
        path[1] = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
        path[2] = tokenOut;
        if (
            (UNIV2_FACTORY.getPair(path[0], path[1])) == address(0) ||
            (UNIV2_FACTORY.getPair(path[1], path[2]) == address(0))
        ) {
            return false;
        }
        return true;
    }

    ///@notice function to calculate the amount of token out and fees in uniswap v2 and , given the amount in at a particular time
    ///@param tokenIn The token that we want to swap with
    ///@param tokenOut The address of the token that we want to get after swapping
    ///@param amountIn The amount of the tokenIn that we want to swap with
    ///@param flag 0 for uniswap rate and 1 for sushiswap rate except for these it (returns 0.0)
    function univ2andsushiswaprate(
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint8 flag
    ) private view returns (uint amount) {
        uint[] memory amountOut;
        address[] memory tokens = new address[](2);
        tokens[0] = tokenIn;
        tokens[1] = tokenOut;
        if (flag == 0) {
            address pairpool = UNIV2_FACTORY.getPair(tokenIn, tokenOut);
            if (pairpool == address(0)) {
                return 0;
            }
            amountOut = UNIV2_SWAP_ROUTER.getAmountsOut(amountIn, tokens);
        } else {
            address pairpool = SUSHI_FACTORY.getPair(tokenIn, tokenOut);
            if (pairpool == address(0)) {
                return 0;
            }
            amountOut = SUSHI_SWAP_ROUTER.getAmountsOut(amountIn, tokens);
        }
        amount = amountOut[1];
    }

    ///@notice function to calculate the amount of token out with weth as connector token
    ///@param tokenIn The token that we want to swap with
    ///@param tokenOut The address of the token that we want to get after swapping
    ///@param amountIn The amount of the tokenIn that we want to swap with
    function univ2ratewithweth(
        address tokenIn,
        address tokenOut,
        uint256 amountIn
    ) private view returns (uint256) {
        address[] memory tokens = new address[](3);
        tokens[0] = tokenIn;
        tokens[1] = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
        tokens[2] = tokenOut;

        try UNIV2_SWAP_ROUTER.getAmountsOut(amountIn, tokens) returns (
            uint256[] memory prices
        ) {
            return prices[2];
        } catch Error(string memory /*reason*/) {
            return 0;
        } catch Panic(uint) {
            return 0;
        } catch (bytes memory) {
            return 0;
        }
    }

    ///@notice function to calculate the amount of token out with weth as connector token
    ///@param tokenIn The token that we want to swap with
    ///@param tokenOut The address of the token that we want to get after swapping
    ///@param amountIn The amount of the tokenIn that we want to swap with
    function getRates(
        address tokenIn,
        address tokenOut,
        uint256 amountIn
    ) private returns (uint bestamountout, uint8 platformflag) {
        address[] memory tokens = new address[](2);
        tokens[0] = tokenIn;
        tokens[1] = tokenOut;
        uint[4] memory amountouts;

        amountouts[0] = getuniswapv3amount(tokens, amountIn);
        amountouts[1] = univ2andsushiswaprate(tokenIn, tokenOut, amountIn, 0);
        amountouts[2] = univ2andsushiswaprate(tokenIn, tokenOut, amountIn, 1);
        amountouts[3] = univ2ratewithweth(tokenIn, tokenOut, amountIn);
        console.log(amountouts[0], amountouts[1], amountouts[2], amountouts[3]);

        if (
            amountouts[0] > amountouts[1] &&
            amountouts[0] > amountouts[2] &&
            amountouts[0] > amountouts[3]
        ) {
            return (amountouts[0], 0);
        }

        if (amountouts[1] > amountouts[2] && amountouts[1] > amountouts[3]) {
            return (amountouts[1], 1);
        }

        if (amountouts[2] > amountouts[3]) {
            return (amountouts[2], 2);
        }
        return (amountouts[3], 3);
    }

    ///@notice function to check the balance of pool address for a particular token
    ///@param token The token address that we want to check amount of in pool
    ///@param pool The address of the pool
    function checkAmount(
        address token,
        address pool
    ) private view returns (uint256) {
        return IERC20Upgradeable(token).balanceOf(pool);
    }

    function univ2swapwithweth(
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint256 amountOutMin,
        address to
    ) private returns (uint) {
        IERC20Upgradeable(tokenIn).safeTransferFrom(
            msg.sender,
            address(this),
            amountIn
        );
        IERC20Upgradeable(tokenIn).safeApprove(
            address(UNIV2_SWAP_ROUTER),
            amountIn
        );

        address[] memory path;
        uint[] memory prices;
        path = new address[](3);
        path[0] = tokenIn;
        path[1] = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
        path[2] = tokenOut;

        prices = UNIV2_SWAP_ROUTER.swapExactTokensForTokens(
            amountIn,
            amountOutMin,
            path,
            to,
            block.timestamp
        );

        return prices[2];
    }

    ///@notice Function to swap token in uniswap v2 and sushiswap
    ///@param tokenIn The token that we want to swap with
    ///@param tokenOut The address of the token that we want to get after swapping
    ///@param amountIn The amount of the tokenIn that we want to swap with
    ///@param amountOutMin The minimum amount of tokenOut tokens that we want
    ///@param to the address that we want the tokenOut tokens to go to after sswap
    ///@param flag 2 for swapping in uniswap v2 and 3 for sushiswap
    function swapuniv2andsushi(
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint256 amountOutMin,
        address to,
        uint256 flag
    ) private {
        require(flag == 1 || flag == 2, "Incorrect Flag");
        IERC20Upgradeable(tokenIn).safeTransferFrom(
            msg.sender,
            address(this),
            amountIn
        );

        address[] memory path;

        path = new address[](2);
        path[0] = tokenIn;
        path[1] = tokenOut;
        if (flag == 1) {
            IERC20Upgradeable(tokenIn).safeApprove(
                address(UNIV2_SWAP_ROUTER),
                amountIn
            );
            UNIV2_SWAP_ROUTER.swapExactTokensForTokens(
                amountIn,
                amountOutMin,
                path,
                to,
                block.timestamp
            );
        } else if (flag == 2) {
            IERC20Upgradeable(tokenIn).safeApprove(
                address(SUSHI_SWAP_ROUTER),
                amountIn
            );
            SUSHI_SWAP_ROUTER.swapExactTokensForTokens(
                amountIn,
                amountOutMin,
                path,
                to,
                block.timestamp
            );
        }
    }

    ///@notice Function to swap token in uniswap v3
    ///@param tokenIn The token that we want to swap with
    ///@param tokenOut The address of the token that we want to get after swapping
    ///@param amountIn The amount of the tokenIn that we want to swap with
    ///@param amountOutMin The minimum amount of tokenOut tokens that we want
    ///@param fee The feee that we want to use for swap
    ///@param to The address that we want the tokenOut tokens to go to after sswap
    function swapuniv3(
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint256 amountOutMin,
        uint24 fee,
        address to
    ) private returns (uint256 amountOut) {
        IERC20Upgradeable(tokenIn).safeTransferFrom(
            msg.sender,
            address(this),
            amountIn
        );

        IERC20Upgradeable(tokenIn).safeApprove(
            address(SWAP_ROUTER_02),
            amountIn
        );

        IV3SwapRouter.ExactInputSingleParams memory params = IV3SwapRouter
            .ExactInputSingleParams({
                tokenIn: tokenIn,
                tokenOut: tokenOut,
                fee: fee,
                recipient: to,
                amountIn: amountIn,
                amountOutMinimum: amountOutMin,
                sqrtPriceLimitX96: 0
            });

        amountOut = SWAP_ROUTER_02.exactInputSingle(params);
    }

    ///@notice Function to swap token in uniswap v3
    ///@param tokenIn The token that we want to swap with
    ///@param tokenOut The address of the token that we want to get after swapping
    ///@param amountIn The amount of the tokenIn that we want to swap with
    ///@param to The address that we want the tokenOut tokens to go to after swap
    ///@param slippageallowed The percent of maxiumum slippage allowed
    function bestrateswap(
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        address to,
        uint256 slippageallowed
    ) external returns (uint amountOut) {
        require(tokenIn != tokenOut, "Input different tokens");
        require(amountIn > 0, "Input amount should be greater than 0");
        require(to != address(0), "Receving address should not be zero");
        require(slippageallowed >= 0, "Slippage should not be zero");

        uint256 balance0;
        uint256 balance1;

        //amount of output token with the  receiver before swapping
        balance0 = checkAmount(tokenOut, to);
        // flag to identify the dex for the swapping
        uint8 flag;
        //fees charged by the dex

        uint256 bestexpectedamountout;
        //getting the flag and fees for the exchange
        //notice- The fees will only be used if the flag is 1, which represents uniswapv3

        (bestexpectedamountout, flag) = getRates(tokenIn, tokenOut, amountIn);
        if (bestexpectedamountout == 0) {
            return 0;
        }
        console.log(bestexpectedamountout, flag);

        uint bestexpectedamountoutwithslippage = (bestexpectedamountout *
            (10000 - slippageallowed)) / 10000;

        if (flag == 0) {
            swapuniv3(
                tokenIn,
                tokenOut,
                amountIn,
                bestexpectedamountoutwithslippage,
                uint24(3000),
                to
            );
        }
        if (flag == 1) {
            swapuniv2andsushi(
                tokenIn,
                tokenOut,
                amountIn,
                bestexpectedamountoutwithslippage,
                to,
                1
            );
        }
        if (flag == 2) {
            swapuniv2andsushi(
                tokenIn,
                tokenOut,
                amountIn,
                bestexpectedamountoutwithslippage,
                to,
                2
            );
        }
        if (flag == 3) {
            univ2swapwithweth(
                tokenIn,
                tokenOut,
                amountIn,
                bestexpectedamountoutwithslippage,
                to
            );
        }

        balance1 = checkAmount(tokenOut, to);
        amountOut = balance1 - balance0;
        console.log(amountOut);
    }
}

