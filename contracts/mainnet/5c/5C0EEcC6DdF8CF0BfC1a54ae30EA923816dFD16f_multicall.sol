contract multicall {

    struct metadata
    {
        string symbol;
        string name;
    }

    function isERC721(address[] memory _addresses) public view returns (bool[] memory returnData) {
        returnData = new bool[](_addresses.length);
        for (uint i=0; i<_addresses.length; i++) {
            try IERC721Metadata(_addresses[i]).supportsInterface(0x80ac58cd) returns (bool _res) { 
                returnData[i] = _res;
            } catch (bytes memory) {
                returnData[i] = false;
            }
        }
    }

    function getTokensMetadata(address[] memory _addresses) public view returns (metadata[] memory returnData) {
        returnData = new metadata[](_addresses.length);
        for (uint i=0; i<_addresses.length; i++) {

            try IERC721Metadata(_addresses[i]).name() returns (string memory _res) {
                returnData[i].name = _res;
            } catch (bytes memory) {
            }

            try IERC721Metadata(_addresses[i]).symbol() returns (string memory _res) {
                returnData[i].symbol = _res;
            } catch (bytes memory) {
            }

        }
    }

}