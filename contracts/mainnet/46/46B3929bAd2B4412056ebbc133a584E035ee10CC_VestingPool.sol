        // Vesting data for seed category
        vesting[seedCategory].limit = 22522500 ether;
        vesting[seedCategory].released = 22522500 ether;
        vesting[seedCategory].scheme = [
            /* initial amount */
            5630625 ether,
            /* M+1 M+2 M+3 M+4 M+5 */
            3378375 ether,
