    }

    /// @notice Set royalty for marketplaces complying with ERC2981 standard
    function setDefaultRoyalty(address receiver, uint96 feeNumerator) public onlyAdmin {
        _setDefaultRoyalty(receiver, feeNumerator);
    }
}

