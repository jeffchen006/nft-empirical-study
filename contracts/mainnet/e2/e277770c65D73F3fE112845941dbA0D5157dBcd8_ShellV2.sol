// File: contracts/SHELLV2.sol


pragma solidity ^0.8.7;






interface ITerraCrate is IERC721AQueryable {
    function tokensOfOwner(address owner)
        external
        view
        override
        returns (uint256[] memory);
}

interface IStaking is IERC721Enumerable {
    function amountOfTurtlesStaked(address owner)
        external
        view
        returns (uint256);
}

contract ShellV2 is ERC20Burnable, Ownable {
    uint256 public constant MAX_SUPPLY = 26_406_000;
    uint256 internal constant EMISSIONS_RATE = 23148140000000;

    address nullAddress = 0x0000000000000000000000000000000000000000;
    address deadAddress = 0x000000000000000000000000000000000000dEaD;

    address private terraCrateAddress;
    address private oldAddress;
    address private stakingAddress;
    address private terrapinsAddress;

    mapping(uint256 => bool) public claimedTerraCrate;
    mapping(address => bool) public claimedWallets;

    mapping(uint256 => uint256) internal tokenIdToTimeStamp;
    mapping(uint256 => address) internal tokenIdToStaker;
    mapping(address => uint256[]) internal stakerToTokenIds;

    mapping(address => bool) public isWalletQuesting;
    mapping(address => uint256) public questTimeStamp;
    mapping(address => uint256) public questPack;
    mapping(address => bool) internal questFound;
    mapping(address => uint256[]) internal questInventory;

    uint256 public totalStakingSupply = 0;

    constructor() ERC20("Shell", "SHELL") {}

    function claimShell() public {
        uint256[] memory getCrates = getClaimableCrates(msg.sender);
        uint256 getShell = getClaimableShell(msg.sender);

        for (uint256 index = 0; index < getCrates.length; index++) {
            claimedTerraCrate[getCrates[index]] = true;
        }

        uint256 claimableShell;
        if (getShell > 0 && !claimedWallets[msg.sender] && block.timestamp < 1679778000) {
            claimableShell = getShell;
            claimedWallets[msg.sender] = true;
        }

        require(getCrates.length > 0 || claimableShell > 0, "cannot be zero!");

        _safeMint(msg.sender, (getCrates.length * 1000 ether) + claimableShell);
    }

    function stakeById(uint256[] memory tokenIds) external {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                IERC721Enumerable(terrapinsAddress).ownerOf(tokenIds[i]) == msg.sender &&
                    tokenIdToStaker[tokenIds[i]] == nullAddress,
                "Token must be stakable by you!"
            );

            stakerToTokenIds[msg.sender].push(tokenIds[i]);

            tokenIdToTimeStamp[tokenIds[i]] = block.timestamp;
            tokenIdToStaker[tokenIds[i]] = msg.sender;

            totalStakingSupply++;
        }
    }

    function unStakeById(uint256[] memory tokenIds) external {
        uint256 totalShell = 0;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                tokenIdToStaker[tokenIds[i]] == msg.sender,
                "Message Sender was not original staker!"
            );

            totalShell =
                totalShell +
                ((block.timestamp - tokenIdToTimeStamp[tokenIds[i]]) *
                    EMISSIONS_RATE);

            removeTokenIdFromStaker(msg.sender, tokenIds[i]);
            tokenIdToStaker[tokenIds[i]] = nullAddress;

            totalStakingSupply--;
        }

        _safeMint(msg.sender, totalShell);
    }

    function claimAll() public {
        uint256[] memory tokenIds = stakerToTokenIds[msg.sender];
        uint256 totalShell = 0;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                tokenIdToStaker[tokenIds[i]] == msg.sender,
                "Token is not claimable by you!"
            );

            totalShell =
                totalShell +
                ((block.timestamp - tokenIdToTimeStamp[tokenIds[i]]) *
                    EMISSIONS_RATE);

            tokenIdToTimeStamp[tokenIds[i]] = block.timestamp;
        }

        _safeMint(msg.sender, totalShell);
    }

    function startQuest(uint256 _questPack) external {
        require(stakerToTokenIds[msg.sender].length > 0, "not staking");
        require(!isWalletQuesting[msg.sender], "you are questing already");
        require(_questPack > 0 && _questPack <= 3, "not more than 3 items");

        uint256 totalBurn;
        if (_questPack == 1) {
            totalBurn = 8 ether;
        } else if (_questPack == 2) {
            totalBurn = 15 ether;
        } else {
            totalBurn = 20 ether;
        }

        isWalletQuesting[msg.sender] = true;
        questTimeStamp[msg.sender] = block.timestamp + 43200;
        questPack[msg.sender] = _questPack;
        questFound[msg.sender] = false;

        transfer(deadAddress, totalBurn);
    }

    function revealItem() public {
        require(isWalletQuesting[msg.sender], "You are not questing.");
        require(block.timestamp >= questTimeStamp[msg.sender], "still Questing");
        require(questFound[msg.sender] == false, "Item has been revealed.");
    
        questFound[msg.sender] = true;

        uint256 random = uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty, msg.sender))) % 100 + 1;
        uint256[] memory probabilities = getQuestPackProb(questPack[msg.sender]);

        if (random <= probabilities[0]) {
            questInventory[msg.sender].push(1);
        } else if (random <= probabilities[0] + probabilities[1]) {
            questInventory[msg.sender].push(2);
        } else if (random <= probabilities[0] + probabilities[1] + probabilities[2]) {
            questInventory[msg.sender].push(3);
        } else if (random <= probabilities[0] + probabilities[1] + probabilities[2] + probabilities[3]) {
            questInventory[msg.sender].push(4);
        } else {
            questInventory[msg.sender].push(5);
        }

        isWalletQuesting[msg.sender] = false;
    }

    function sellAll() public {
        require(questInventory[msg.sender].length > 0, "zero inventory");

        uint256[] memory inventory = questInventory[msg.sender];
        uint256 totalSHELL = 0;
        uint256 tax = 1 ether;
        
        for (uint i = 0; i < inventory.length; i++) {
            if (inventory[i] == 1) { // Gem
                totalSHELL = totalSHELL + 100 ether;
            } else if (inventory[i] == 2) { // Gold
                totalSHELL = totalSHELL + 85 ether;
            } else if (inventory[i] == 3) { // Silver
                totalSHELL = totalSHELL + 65 ether;
            } else if (inventory[i] == 4) { // Stone
                totalSHELL = totalSHELL + 25 ether;
            } else if (inventory[i] == 5) { // Wood
                totalSHELL = totalSHELL + 10 ether;
            } else if (inventory[i] == 6) { // Ring
                totalSHELL = totalSHELL + 280 ether;
            } else if (inventory[i] == 7) { // Gold Chain
                totalSHELL = totalSHELL + 210 ether;
            } else if (inventory[i] == 8) { // Silver Chain
                totalSHELL = totalSHELL + 160 ether;
            } else if (inventory[i] == 9) { // Axe
                totalSHELL = totalSHELL + 110 ether;
            }
            
            removeItemFromInventory(msg.sender, inventory[i]);
        }

        _safeMint(msg.sender, totalSHELL - tax);
    }

    function CraftTool(uint256 craftItem) public {
        require(questInventory[msg.sender].length > 0, "zero inventory");
        require(craftItem > 0 && craftItem <= 4, "Invalid craft item");

        uint256[] memory requiredItems;
        uint256 item;
        if (craftItem == 1) {
            require(countItem(1) >= 2, "Not enought material");
            requiredItems = new uint256[](2);
            requiredItems[0] = 1;
            requiredItems[1] = 1;

            item = 6;
        } else if (craftItem == 2) {
            require(countItem(2) >= 2, "Not enought material");
            requiredItems = new uint256[](2);
            requiredItems[0] = 2;
            requiredItems[1] = 2;
            item = 7;
        } else if (craftItem == 3) {
            require(countItem(3) >= 2, "Not enought material");
            requiredItems = new uint256[](2);
            requiredItems[0] = 3;
            requiredItems[1] = 3;
            item = 8;
        } else if (craftItem == 4) {
            require(countItem(4) >= 2, "Not enought material");
            require(countItem(5) >= 3, "Not enought material");
            requiredItems = new uint256[](5);
            requiredItems[0] = 4;
            requiredItems[1] = 4;
            requiredItems[2] = 5;
            requiredItems[3] = 5;
            requiredItems[4] = 5;
            item = 9;
        }

        for (uint256 i = 0; i < requiredItems.length; i++) {
            removeItemFromInventory(msg.sender, requiredItems[i]);
        }

        questInventory[msg.sender].push(item);

        transfer(deadAddress, 2 ether);
    }

    function countItem(uint256 itemId) internal view returns (uint256) {
        uint256[] memory inventory = questInventory[msg.sender];
        uint256 count = 0;
        
        for (uint256 i = 0; i < inventory.length; i++) {
            if (inventory[i] == itemId) {
                count++;
            }
        }
        return count;
    }

    function getQuestPackProb(uint256 _questPack) internal pure returns (uint256[] memory) {
        uint256[] memory probabilities = new uint256[](5);

        if (_questPack == 1) {
            probabilities[0] = 3; 
            probabilities[1] = 6; 
            probabilities[2] = 10; 
            probabilities[3] = 25;
            probabilities[4] = 56; 
        } else if (_questPack == 2) {
            probabilities[0] = 4; 
            probabilities[1] = 7; 
            probabilities[2] = 15;
            probabilities[3] = 30;
            probabilities[4] = 44;
        } else if (_questPack == 3) {
            probabilities[0] = 5;
            probabilities[1] = 8;
            probabilities[2] = 18;
            probabilities[3] = 33;
            probabilities[4] = 36;
        }

        return probabilities;
    }

    function getAllRewards(address staker) public view returns (uint256) {
        uint256[] memory tokenIds = stakerToTokenIds[staker];
        uint256 totalShell = 0;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            totalShell =
                totalShell +
                ((block.timestamp - tokenIdToTimeStamp[tokenIds[i]]) *
                    EMISSIONS_RATE);
        }

        return totalShell;
    }

    function getRewardsByTokenId(uint256 tokenId) public view returns (uint256) {
        require(
            tokenIdToStaker[tokenId] != nullAddress,
            "Token is not staked!"
        );

        uint256 secondsStaked = block.timestamp - tokenIdToTimeStamp[tokenId];

        return secondsStaked * EMISSIONS_RATE;
    }

    function remove(address staker, uint256 index) internal {
        if (index >= stakerToTokenIds[staker].length) return;

        for (uint256 i = index; i < stakerToTokenIds[staker].length - 1; i++) {
            stakerToTokenIds[staker][i] = stakerToTokenIds[staker][i + 1];
        }
        stakerToTokenIds[staker].pop();
    }

    function removeTokenIdFromStaker(address staker, uint256 tokenId) internal {
        for (uint256 i = 0; i < stakerToTokenIds[staker].length; i++) {
            if (stakerToTokenIds[staker][i] == tokenId) {
                //This is the tokenId to remove;
                remove(staker, i);
            }
        }
    }

    function removeItem(address staker, uint256 index) internal {
        if (index >= questInventory[staker].length) return;

        for (uint256 i = index; i < questInventory[staker].length - 1; i++) {
            questInventory[staker][i] = questInventory[staker][i + 1];
        }
        questInventory[staker].pop();
    }

    function removeItemFromInventory(address staker, uint256 tokenId) internal {
        for (uint256 i = 0; i < questInventory[staker].length; i++) {
            if (questInventory[staker][i] == tokenId) {
                //This is the tokenId to remove;
                removeItem(staker, i);
                return; 
            }
        }
    }

    function getClaimableShell(address _owner) public view returns (uint256) {
        uint256 walletCount = IERC721Enumerable(oldAddress).balanceOf(_owner);
        uint256 stakingCount = IStaking(stakingAddress).amountOfTurtlesStaked(_owner);
        
        uint256 walletClaimableShell = walletCount * 250 ether;
        uint256 stakingClaimableShell = stakingCount * 1000 ether;
        uint256 totalShell = walletClaimableShell + stakingClaimableShell;
        
        return totalShell;
    }

    function getClaimableCrates(address _owner) public view returns (uint256[] memory) {
        uint256[] memory tokens = ITerraCrate(terraCrateAddress).tokensOfOwner(_owner);
        uint256 claimableTokensCount;
        for (uint256 index = 0; index < tokens.length; index++) {
            uint256 tokenId = tokens[index];
            if (!claimedTerraCrate[tokenId]) {
                claimableTokensCount++;
            }
        }

        uint256[] memory claimableTokens = new uint256[](claimableTokensCount);
        uint256 resultsIndex;
        for (uint256 index = 0; index < tokens.length; index++) {
            uint256 tokenId = tokens[index];
            if (!claimedTerraCrate[tokenId]) {
                claimableTokens[resultsIndex] = tokenId;
                resultsIndex++;
            }
        }
        return claimableTokens;
    }

    function getTokensStaked(address staker) public view returns (uint256[] memory) {
        return stakerToTokenIds[staker];
    }

    function getInventory(address quester) public view returns (uint256[] memory) {
        return questInventory[quester];
    }

    function getStaker(uint256 tokenId) public view returns (address) {
        return tokenIdToStaker[tokenId];
    }

    function setAddresses(address _terraCrate, address _old, address _staking) external onlyOwner { 
        terraCrateAddress = _terraCrate;
        oldAddress = _old;
        stakingAddress = _staking;
    }

    function setTerrapinAddress(address _terrapins) external onlyOwner{
        terrapinsAddress = _terrapins;
    }

    function _safeMint(address to, uint256 amount) internal {
        uint256 newSupply = totalSupply() + amount;
        require(newSupply <= MAX_SUPPLY * 1 ether, "Max Supply.");
        _mint(to, amount);
    }
}