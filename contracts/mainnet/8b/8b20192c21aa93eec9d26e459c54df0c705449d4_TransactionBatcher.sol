pragma solidity 0.5.12;
pragma experimental ABIEncoderV2;

contract TransactionBatcher {

    event Done();

    function batchSend(address[] memory targets, uint[] memory values, bytes[] memory datas) public payable {
        for (uint i = 0; i < targets.length; i++) {
            (bool success,) = targets[i].call.value(values[i])(datas[i]);
            if (!success) revert('transaction failed gayu');
        }
        emit Done();
    }

    function payToContract() public payable {}

    function transferBack(address payable to) public{
        to.transfer(address(this).balance);
    }

   
}
