}

interface Pair {
    function swap(
        uint256 amount0Out,
        uint256 amount1Out,
        address to,
        bytes calldata data
    ) external;
}

library SafeMath {
    function add(uint256 x, uint256 y) internal pure returns (uint256 z) {
        require((z = x + y) >= x, "ds-math-add-overflow");
    }

    function sub(uint256 x, uint256 y) internal pure returns (uint256 z) {
        require((z = x - y) <= x, "ds-math-sub-underflow");
    }

    function mul(uint256 x, uint256 y) internal pure returns (uint256 z) {
        require(y == 0 || (z = x * y) / y == x, "ds-math-mul-overflow");
    }
}

contract PairInteraction {
    using SafeMath for uint256;

    function swap(
        address _pool,
        uint256 amount0Out,
        uint256 amount1Out,
        address to,
        bytes calldata data
    ) external {
        Pair(_pool).swap(amount0Out, amount1Out, to, data);
    }

    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    )
        external
        pure
        returns (
            uint256 amountOut,
            uint256 _numerator,
            uint256 _denominator,
            uint256 _amountInWithFee
        )
    {
        _amountInWithFee = amountIn.mul(997);
        _numerator = _amountInWithFee.mul(reserveOut);
        _denominator = reserveIn.mul(1000).add(_amountInWithFee);
        amountOut = _numerator / _denominator;
    }

    function mul(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) external pure returns (uint256 amountOut) {
        amountOut = amountIn.mul(997);
    }

    function mul2(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) external pure returns (uint256 amountOut) {
        uint256 amountInWithFee = amountIn.mul(997);
        amountOut = amountInWithFee.mul(reserveOut);
    }

    function add(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) external pure returns (uint256 amountOut) {
        uint256 amountInWithFee = amountIn.mul(997);
        uint256 numerator = amountInWithFee.mul(reserveOut);
        amountOut = reserveIn.mul(1000).add(amountInWithFee);
    }

    function div(uint256 numerator, uint256 denominator)
        external
        pure
        returns (uint256 amountOut)
    {
        amountOut = numerator / denominator;
    }
}

