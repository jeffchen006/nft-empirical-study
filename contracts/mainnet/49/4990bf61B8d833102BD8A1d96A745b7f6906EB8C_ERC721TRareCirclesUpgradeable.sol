pragma solidity ^0.8.0;
pragma abicoder v2;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/IERC165Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import "./IERC721TRareCirclesUpgradeable.sol";
import "./ERC721TURIUpgradeable.sol";
import "./RoyaltyUpgradeable.sol";
import "./ERC721TEnumerableUpgradeable.sol";

contract ERC721TRareCirclesUpgradeable is
    IERC721TRareCirclesUpgradeable,
    ContextUpgradeable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    ERC721TUpgradeable,
    ERC721TURIUpgradeable,
    ERC721TEnumerableUpgradeable,
    RoyaltyUpgradeable
{
    using SafeMathUpgradeable for uint256;
    
    event CreateERC721TRareCircles(
        address indexed owner,
        string name,
        string symbol
    );

    event BaseURI(string uri);
    event Name(string name);
    event Symbol(string symbol);
    event Manager(address manager);

    // =============================================================
    //                            STORAGE
    // =============================================================

    // Manager address
    address public manager;

    // Indicates that the contract has been setup after creation.
    bool private _setup;

    // =============================================================
    //                          INITIALIZATION
    // =============================================================
    function __ERC721TRareCircles_init(
        string memory _name,
        string memory _symbol,
        string memory _baseURI
    ) external virtual {
        __ERC721TRareCircles_init_unchained(
            _name,
            _symbol,
            _baseURI
        );

        emit CreateERC721TRareCircles(_msgSender(), _name, _symbol);
    }

    function __ERC721TRareCircles_init_unchained(
        string memory _name,
        string memory _symbol,
        string memory _baseURI
    ) internal initializer {
        _setBaseURI(_baseURI);

        __Context_init_unchained();
        __UUPSUpgradeable_init_unchained();
        __Ownable_init_unchained();
        __ERC721TURI_init_unchained();
        __ERC165_init_unchained();
        __ERC721T_init_unchained(_name, _symbol);
        __Royalty_init_unchained();
    }

    // =============================================================
    //                          UPGRADEABLE
    // =============================================================

    function _authorizeUpgrade(address) internal override onlyOwner {}

    // =============================================================
    //                            IERC165
    // =============================================================
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(
            ERC721TUpgradeable,
            ERC721TEnumerableUpgradeable,
            RoyaltyUpgradeable
        )
        returns (bool)
    {
        return
            ERC721TUpgradeable.supportsInterface(interfaceId) ||
            ERC721TEnumerableUpgradeable.supportsInterface(interfaceId) ||
            RoyaltyUpgradeable.supportsInterface(interfaceId);
    }

    // =============================================================
    //                        MINT OPERATIONS
    // =============================================================
    function mint(address to, uint256 index) external override onlyManager {
        _safeMint(to, index);
    }

    // =============================================================
    //                        METADATA
    // =============================================================
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override(ERC721TUpgradeable, ERC721TURIUpgradeable)
        returns (string memory)
    {
        return ERC721TURIUpgradeable.tokenURI(tokenId);
    }

    function setTokenURI(uint256 _tokenId, string memory _tokenURI)
        external
        onlyOwner
    {
        return ERC721TURIUpgradeable._setTokenURI(_tokenId, _tokenURI);
    }

    function baseTokenURI() public view returns (string memory) {
        return super.baseURI();
    }

    function setBaseTokenURI(string memory baseTokenURI_)
        external
        onlyOwner
    {
        _setBaseURI(baseTokenURI_);
        emit BaseURI(baseTokenURI_);
    }

    function setName(string memory name_) external onlyOwner {
        _setName(name_);
        emit Name(name_);
    }

    function setSymbol(string memory symbol_) external onlyOwner {
        _setSymbol(symbol_);
        emit Symbol(symbol_);
    }

    function tokenOfOwnerByIndex(address owner, uint256 index)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return super.tokenOfOwnerByIndex(owner, index);
    }

    function tokensOfOwner(address owner)
        public
        view
        virtual
        override
        returns (uint256[] memory)
    {
        return super.tokensOfOwner(owner);
    }

    function tokenByIndex(uint256 index)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return super.tokenByIndex(index);
    }

    function totalSupply() view public override (IERC721TRareCirclesUpgradeable, ERC721TEnumerableUpgradeable) returns (uint256) {
        return ERC721TEnumerableUpgradeable.totalSupply();
    }

    // =============================================================
    //                     Royalty
    // =============================================================
    function setRoyalty(address creator_, uint256 amount_)
        external
        onlyOwner
    {
        _saveRoyalty(LibPart.Part(creator_, amount_));
    }

    // =============================================================
    //                     SETUP
    // =============================================================
    function setup(address _creator, uint256 _amount, address _manager) external onlyOwner {
        require(!_setup,"ERC721T: contract is already setup");
        require(_manager != address(0), "ERC721T: manager not set");
        _setup = true;
        _saveRoyalty(LibPart.Part(_creator, _amount));


        manager = _manager;
        emit Manager(manager);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721TUpgradeable, ERC721TEnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    modifier onlyManager() {
        require(
            manager == _msgSender(),
            "ERC721T: Caller is not a manager"
        );
        _;
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[48] private __gap;
}

