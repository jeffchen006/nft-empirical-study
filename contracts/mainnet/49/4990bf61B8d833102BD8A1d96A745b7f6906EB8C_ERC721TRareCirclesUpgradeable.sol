pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "./LibPart.sol";

abstract contract RoyaltyUpgradeable is Initializable, ContextUpgradeable {
    struct Part {
        address payable account;
        uint96 value;
    }

    event RoyaltySet(LibPart.Part royalty);
    // =============================================================
    //                            CONSTANT
    // =============================================================

    bytes4 private constant _INTERFACE_ID_ERC165 = 0x01ffc9a7;
    /// bytes4(keccak256("royaltyInfo(uint256,uint256)")) == 0x2a55205a
    bytes4 private constant _INTERFACE_ID_ROYALTIES = 0x2a55205a;
    uint96 constant _WEIGHT_VALUE = 1000000;

    // =============================================================
    //                            STORAGE
    // =============================================================

    LibPart.Part public royalty;

    // =============================================================
    //                          INITIALIZATION
    // =============================================================
    function __Royalty_init() internal onlyInitializing {}

    function __Royalty_init_unchained() internal onlyInitializing {}

    // =============================================================
    //                            IERC165
    // =============================================================
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        returns (bool)
    {
        return
            interfaceId == _INTERFACE_ID_ERC165 ||
            interfaceId == _INTERFACE_ID_ROYALTIES;
    }

    function royaltyInfo(uint256, uint256 _salePrice)
        external
        view
        returns (address receiver, uint256 royaltyAmount)
    {
        if (_salePrice == 0) {
            return (address(0), 0);
        }
        require(royalty.value < 10000, "Royalty total value should be < 10000");
        uint256 amount = ((_salePrice * 100) / _WEIGHT_VALUE) * royalty.value;
        return (royalty.account, amount);
    }

    function _saveRoyalty(LibPart.Part memory _royalty) internal {
        require(
            _royalty.value < 10000,
            "Royalty total value should be < 10000"
        );
        royalty = _royalty;
        emit RoyaltySet(_royalty);
    }

    uint256[49] private __gap;
}

