    function price(string calldata name) public pure returns (uint256) {
        uint256 len = StringUtils.strlen(name);
        require(len > 0);
        if (len == 1) {
            return 0.002 * 10**18;
        } else if (len == 5) {
            return 0.0015 * 10**18;
        } else if (len == 8) {
            return 0.0007 * 10**18;
        } else {
            return 0.0005 * 10**18;
        }
    }

