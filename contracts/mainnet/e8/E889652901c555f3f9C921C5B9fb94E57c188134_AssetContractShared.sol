    // PROXY HELPER METHODS

    function _isProxyForUser(address _user, address _address)
    internal
    view
    virtual
    returns (bool)
    {
        if (!proxyRegistryAddress.isContract()) {
            return false;
        }
        return address(IProxyRegistry(proxyRegistryAddress).proxies(_user)) == _address;
    }

