error CHIZUCollectionFactory_Not_Granted();
error CHIZUCollectionFactory_Nonce_Already_Exist();
error CHIZUCollectionFactory_Time_Expired();

/**
 * @title A factory to create NFT collections.
 * @notice Call this factory to create an NFT collection contract managed by a single creator.
 * @dev This creates and initializes an ERC-1165 minimal proxy pointing to the NFT collection contract template.
 */
contract CHIZUCollectionFactory is ICollectionFactory {
    using AddressUpgradeable for address;
    using AddressUpgradeable for address payable;
    using Clones for address;
    using Strings for uint256;
    using ECDSA for bytes32;

    struct CHIZUImplementation {
        // Accounts of the source to be replicated
        address source;
        // Role to allow
        bytes32 role;
        // Whether grant is required
        bool needGranted;
        // Whether signature is required
        bool needSignature;
        // Whether deprecation is possible
        bool deprecated;
        // the version of implementation
        uint32 version;
    }

    struct ValidateInfo {
        address signer;
        bytes32 hashValue;
        bytes signature;
    }

    CHIZUImplementation public implementation;

    /**
     * @notice The baseURI to use for the tokenURI
     */
    string internal _baseURI;

    /**
     * @notice The contract address which manages common roles.
     */
    IRoles public chizuCore;

    /**
     * @dev Keep the Signature Log to prevent duplicated request
     * @dev There is almost no chance for different order to have same finalHash
     */
    mapping(uint256 => bool) public sigHistory;

    /**
     * @notice Emitted when a new collection is created from this factory.
     * @param collectionContract The address of the new NFT collection contract.
     * @param creator The address of the creator which owns the new collection.
     * @param version The implementation version used by the new collection.
     * @param name The name of the collection contract created.
     * @param symbol The symbol of the collection contract created.
     * @param nonce The nonce used by the creator when creating the collection,
     * used to define the address of the collection.
     */
    event CollectionCreated(
        address indexed collectionContract,
        address indexed creator,
        uint256 indexed version,
        string name,
        string symbol,
        uint256 nonce
    );

    /**
     * @notice Emitted when a new collection is created with signatrue from this factory.
     * @param collectionContract The address of the new NFT collection contract.
     * @param creator The address of the creator which owns the new collection.
     * @param version The implementation version used by the new collection.
     * @param name The name of the collection contract created.
     * @param symbol The symbol of the collection contract created.
     * @param nonce The nonce used by the creator when creating the collection,
     * @param collectionHash Hash for collection creation
     */
    event CollectionCreatedWithSignature(
        address indexed collectionContract,
        address indexed creator,
        uint256 indexed version,
        string name,
        string symbol,
        uint256 nonce,
        bytes32 collectionHash
    );

    /**
     * @notice Emitted when the implementation contract used by new collections is updated.
     * @param implementation The new implementation contract address.
     * @param version The version of the new implementation, auto-incremented.
     */
    event ImplementationAdded(
        address indexed implementation,
        uint256 indexed version
    );

    /**
     * @notice Emitted when the implementation contract used by new collections is updated.
     * @param implementation The new implementation contract address.
     * @param version The version of the new implementation, auto-incremented.
     */
    event ImplementationUpdated(
        address indexed implementation,
        uint256 indexed version
    );

    /**
     * @notice Emitted when the contract defining roles is updated.
     * @param chizuCore The new roles contract address.
     */
    event RolesContractUpdated(address indexed chizuCore);

    modifier onlyAdmin() {
        require(
            chizuCore.isAdmin(msg.sender),
            "CHIZUCollectionFactory: Caller does not have the Admin role"
        );
        _;
    }

    /**
     * @param _chizuCore The address of the contract defining roles for collections to use.
     */
    constructor(address _chizuCore) {
        _updateRolesContract(_chizuCore);
    }

    /**
     * @return chizuCore The contract address which manages common roles.
     */
    function rolesContract() external view override returns (IRoles) {
        return chizuCore;
    }

    /**
     * @notice Allows CHIZU to update the collection implementation used for future collections.
     * Existing collections are not impacted.
     * @param role role to allow user to create new collection.
     * @param needGranted If only role user can create.
     * @param needSignature If user should submit signature of role user.
     * @param deprecated If user cannot create new collection of implementation.
     * @param _implementation The new collection implementation address.
     */
    function adminUpdateImplementation(
        string memory role,
        bool needGranted,
        bool needSignature,
        bool deprecated,
        address _implementation
    ) external onlyAdmin {
        bytes32 ROLE = keccak256(abi.encodePacked(role));
        _updateImplementation(
            ROLE,
            needGranted,
            needSignature,
            deprecated,
            _implementation
        );
    }

    /**
     * @notice Allows CHIZU to change the admin role contract address.
     * @param _chizuCore The new admin role contract address.
     */
    function adminUpdateRolesContract(address _chizuCore) external onlyAdmin {
        _updateRolesContract(_chizuCore);
    }

    /**
     * @notice Create a new collection contract.
     * @dev The nonce is required and must be unique for the msg.sender + implementation version,
     * otherwise this call will revert.
     * @param name The name for the new collection being created.
     * @param symbol The symbol for the new collection being created.
     * @param nonce An arbitrary value used to allow a creator to mint multiple collections.
     * @return collectionAddress The address of the new collection contract.
     */
    function createCollection(
        string calldata name,
        string calldata symbol,
        uint256 nonce,
        uint256 expiredAt
    ) external returns (address collectionAddress) {
        if (expiredAt < block.timestamp) {
            revert CHIZUCollectionFactory_Time_Expired();
        }
        if (sigHistory[nonce]) {
            revert CHIZUCollectionFactory_Nonce_Already_Exist();
        }
        require(
            bytes(symbol).length != 0,
            "CHIZUCollectionFactory: Symbol is required"
        );

        CHIZUImplementation memory _implementation = implementation;

        require(
            !_implementation.deprecated,
            "CHIZUCollectionFactory: Implementation is deprecated"
        );
        require(
            !_implementation.needSignature,
            "CHIZUCollectionFactory: Signature is required"
        );
        if (
            _implementation.needGranted &&
            !chizuCore.hasGranted(_implementation.role, msg.sender)
        ) {
            revert CHIZUCollectionFactory_Not_Granted();
        }
        // This reverts if the NFT was previously created using this implementation version + msg.sender + nonce
        collectionAddress = _implementation.source.cloneDeterministic(
            _getSalt(msg.sender, nonce)
        );

        IDistinctiveCollection(collectionAddress).initialize(
            payable(msg.sender),
            name,
            symbol,
            address(chizuCore)
        );

        sigHistory[nonce] = true;

        emit CollectionCreated(
            collectionAddress,
            msg.sender,
            uint256(_implementation.version),
            name,
            symbol,
            nonce
        );
    }

    /**
     * @notice Create a new collection contract.
     * @dev The nonce is required and must be unique for the msg.sender + implementation version,
     * otherwise this call will revert.
     * @param name The name for the new collection being created.
     * @param symbol The symbol for the new collection being created.
     * @param nonce An arbitrary value used to allow a creator to mint multiple collections.
     * @param validateInfo signer, hash, signature
     * @return collectionAddress The address of the new collection contract.
     */
    function createCollectionWithSignature(
        string calldata name,
        string calldata symbol,
        uint256 nonce,
        ValidateInfo memory validateInfo,
        uint256 expiredAt
    ) external returns (address collectionAddress) {
        if (expiredAt < block.timestamp) {
            revert CHIZUCollectionFactory_Time_Expired();
        }
        if (sigHistory[nonce]) {
            revert CHIZUCollectionFactory_Nonce_Already_Exist();
        }

        require(
            bytes(name).length != 0,
            "CHIZUCollectionFactory: Name is required"
        );
        require(
            bytes(symbol).length != 0,
            "CHIZUCollectionFactory: Symbol is required"
        );

        bool success = _validateColletionSignature(
            validateInfo.signer,
            validateInfo.hashValue,
            validateInfo.signature,
            msg.sender,
            nonce,
            expiredAt
        );
        require(success, "CHIZUCollectionFactory: Signature is wrong");

        CHIZUImplementation memory _implementation = implementation;

        require(
            !_implementation.deprecated,
            "CHIZUCollectionFactory: Implementation is deprecated"
        );

        require(
            chizuCore.hasGranted(_implementation.role, validateInfo.signer),
            "CHIZUCollectionFactory : Valid signer is required"
        );

        if (
            _implementation.needGranted &&
            !chizuCore.hasGranted(_implementation.role, msg.sender)
        ) {
            revert CHIZUCollectionFactory_Not_Granted();
        }

        // This reverts if the NFT was previously created using this implementation version + msg.sender + nonce
        collectionAddress = _implementation.source.cloneDeterministic(
            _getSalt(msg.sender, nonce)
        );

        IDistinctiveCollection(collectionAddress).initialize(
            payable(msg.sender),
            name,
            symbol,
            address(chizuCore)
        );

        sigHistory[nonce] = true;

        emit CollectionCreatedWithSignature(
            collectionAddress,
            msg.sender,
            uint256(_implementation.version),
            name,
            symbol,
            nonce,
            validateInfo.hashValue
        );
    }

    /**
     * @notice Returns the address of a collection given the current implementation version, creator, and nonce.
     * This will return the same address whether the collection has already been created or not.
     * @param creator The creator of the collection.
     * @param nonce An arbitrary value used to allow a creator to mint multiple collections.
     * @return collectionAddress The address of the collection contract that would be created by this nonce.
     */
    function predictCollectionAddress(address creator, uint256 nonce)
        external
        view
        returns (address collectionAddress)
    {
        collectionAddress = implementation.source.predictDeterministicAddress(
            _getSalt(creator, nonce)
        );
    }

    function _updateRolesContract(address _chizuCore) private {
        require(
            _chizuCore.isContract(),
            "CHIZUCollectionFactory: RolesContract is not a contract"
        );
        chizuCore = IRoles(_chizuCore);

        emit RolesContractUpdated(_chizuCore);
    }

    function updateBaseURI(string memory baseURI_) public onlyAdmin {
        _baseURI = baseURI_;
    }

    function baseURI() public view returns (string memory uri) {
        uri = _baseURI;
    }

    /**
     * @dev Updates the implementation address, increments the version, and initializes the template.
     * Since the template is initialized when set, implementations cannot be re-used.
     * To downgrade the implementation, deploy the same bytecode again and then update to that.
     */
    function _updateImplementation(
        bytes32 role,
        bool needGranted,
        bool needSignature,
        bool deprecated,
        address newContract
    ) private {
        CHIZUImplementation memory _implementation = implementation;
        _implementation.role = role;
        _implementation.needGranted = needGranted;
        _implementation.needSignature = needSignature;
        _implementation.deprecated = deprecated;

        unchecked {
            // Version cannot overflow 32 bits.
            _implementation.version++;
        }

        if (newContract.isContract()) {
            _implementation.source = newContract;
            // The implementation is initialized when assigned so that others may not claim it as their own.
            IDistinctiveCollection(newContract).initialize(
                payable(newContract),
                string(
                    abi.encodePacked(
                        "Chizu Collection Template i",
                        "v",
                        (uint256(_implementation.version)).toString()
                    )
                ),
                string(
                    abi.encodePacked(
                        "CCTi",
                        "v",
                        (uint256(_implementation.version)).toString()
                    )
                ),
                address(chizuCore)
            );
            implementation = _implementation;
            emit ImplementationUpdated(
                _implementation.source,
                _implementation.version
            );
        } else {
            implementation = _implementation;
            emit ImplementationUpdated(
                _implementation.source,
                _implementation.version
            );
        }
    }

    /**
     * @dev Functions that validate collection signature
     */
    function _validateColletionSignature(
        address signerAddress,
        bytes32 collectionHash,
        bytes memory collectionSignatures,
        address msgSender,
        uint256 nonce,
        uint256 expiredAt
    ) internal view returns (bool success) {
        bytes32 calculatedHash = keccak256(
            abi.encodePacked(
                uint256(uint160(address(this))),
                uint256(uint160(msgSender)),
                nonce,
                uint256(expiredAt)
            )
        );
        bytes32 calculatedOrigin = keccak256(
            abi.encodePacked(
                "\x19Ethereum Signed Message:\n32",
                uint256(calculatedHash)
            )
        );
        address recoveredSigner = calculatedOrigin.recover(
            collectionSignatures
        );
        if (calculatedHash != collectionHash) {
            return false;
        }

        if (recoveredSigner != signerAddress) {
            return false;
        }
        success = true;
    }

    function _getSalt(address creator, uint256 nonce)
        private
        pure
        returns (bytes32)
    {
        return keccak256(abi.encodePacked(creator, nonce));
    }
}

